//snippet: piano-script
        (function() {
  function init() {
    const useSandbox = window.env.CONTENT_HUB_ENV !== 'prod';
    const metadata = window.CNN?.metadata?.content;
    const pageType = metadata?.pageType;
    const subscriptionOnly = metadata?.enableSubscriptionContent ? "subscription-only" : null;
    const disablePaywall = metadata?.disablePaywall ? "disable-paywall" : null;
    const section = metadata?.section || [];
    const tags = [pageType, ...section, subscriptionOnly, disablePaywall].filter(Boolean);
    const pianoJWT = window.CNN.helpers?.getCookie ? window.CNN.helpers.getCookie('__pianoParams') : undefined;


    window.tp = window.tp || [];
    tp.push(["setUseTinypassAccounts", false]);
    tp.push(["setUsePianoIdUserProvider", false]);
    tp.push(["setUsePianoIdLiteUserProvider", true]);
    tp.push(["setPianoIdUrl", window.env.PIANO_ID_URL]);

    if (useSandbox) {
      tp.push(["setSandbox", useSandbox]);
      tp.push(["setDebug", true]);
    }

    tp.push(["setTags", tags]);

    if (window.env.PIANO_ENABLE_AUTH_SYNC && pianoJWT) {
      tp.push(["setExternalJWT", pianoJWT]);
    }

    addScript({
      src: window.env.PIANO_HOST + window.env.PIANO_APP_ID,
      async: true,
      name: "piano-script",
      ucStates: ["iab-gdpr", "data-share", "data-store", "ads-contextual", "ads-person-prof", "ads-person", "content-person-prof", "content-person", "measure-ads", "measure-content", "measure-market"]
    });
  }

  if (window?.WBD?.UserConsent?.isReady()) {
    init();
  } else {
    document.addEventListener("userConsentReady", init, { once: true });
  }
})();

        
    }())
  </script>

