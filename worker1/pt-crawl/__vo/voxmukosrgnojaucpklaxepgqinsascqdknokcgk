   <script type="text/javascript">


   YAHOO.namespace("adams.container");
YAHOO.util.Event.onDOMReady(function () {
  // Remove progressively enhanced content class, just before creating the module
    YAHOO.util.Dom.removeClass("dialog1", "yui-pe-content");

    var handleCancel = function() {
        this.cancel();
    };

});

      function closeWindow(win){
         if(win.opener != null && win.opener!=win){ //if it's a pop-up window
            win.close();
            return closeWindow(win.opener);
         } else {
            return win;
         }
      }

      function selfFocus() {
         var noPopup="";
         var a = window.open("/adams/blank.html", "test","width=1,height=1");
         if (a){
            a.close();
         }else{
            noPopup="nopopup=true";
         }
         var topWin = self;
         if(self.opener != null){//if it's a pop-up window
            topWin = closeWindow(self);
         }
         if(topWin != topWin.parent){//if it's a frame
            topWin.top.location = "/adams/login.do?prompt=true&"+noPopup;
            return;
         } else {//if it's the top window
            document.login.j_username.focus();
         }
         
         if (noPopup!=""){
            topWin.top.location = "/adams/login.do?prompt=true&"+noPopup;
         }
         

      }

   function showHideCapsOn(e) {
       if (!e) {
           e = window.event;
       }

       var kc = e.keyCode ? e.keyCode : e.which;
       var sk = e.shiftKey ? e.shiftKey : ((kc == 16) ? true : false);
 
       if (((kc >= 65 && kc <= 90) && !sk) || ((kc >= 97 && kc <= 122) && sk)) {
    	   document.getElementById("dialog1_c").style.visibility="visible";
       } else {
    	   document.getElementById("dialog1_c").style.visibility="hidden";
       }
   }


       function verifyFields() {
           if (document.login.j_username.value.length==0) {
               document.getElementById("errorTable").style.display="inline";
               document.getElementById("errorMsgId").innerHTML='Please enter your user name';
               return false;
           }
           document.getElementById("errorTable").style.display="none";
           document.getElementById("errorMsgId").innerHTML="";
           return true;
       }
   </script>
