    <section class="banner-box banner">
        <div class="holder">
            <h1>History</h1>
        </div>
    </section>

<div class="main">
    <div class="main-holder">
        

        <section class="text-content centreCol" itemscope itemtype="http://schema.org/NewsArticle">
            

            <p><strong>
