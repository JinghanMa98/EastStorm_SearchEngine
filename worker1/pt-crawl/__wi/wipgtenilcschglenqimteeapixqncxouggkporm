<div id="story_head">
<div id="story_head_content">
<h1 class="headline" itemprop="headline">Full Body Swimsuit Now Banned for Professional Swimmers</h1>
<div id="byline_container">
<div class="byline_date">
<div class="date">WASHINGTON, Jan. 4, 2010</div>
<div class="byline">By KRISTINA WONG </div>
</div>
<div id="sponsoredByUnit">
<script type="text/javascript">digGetAd("SponsoredByLogo");</script>
</div>
<div class="clearboth"><!-- empty --></div>
</div>
</div>
</div>
<div id="shareTop" class="sharetools-horizontal">
<div class="share-btn fblike">
<fb:like href="http://abcnews.go.com/Politics/full-body-swimsuit-now-banned-professional-swimmers/story?id=9437780" send="false" layout="button_count" width="90" show_faces="false" font="arial"></fb:like>
</div>
<div id="fbshareTop" class="share-btn fbshare">
<script type="text/javascript">
renderFacebookShare();
</script>
</div>
<script type="text/javascript">
var shareTop = new shareNewsObj();
shareTop.options = {renderDiv:'shareTop', //target div
