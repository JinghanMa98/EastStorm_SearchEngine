  }

  #rt-footer-template--26858486727043__rt_footer_9M6VCG .rt-footer-background {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: url('//www.rouleur.cc/cdn/shop/files/Mallorca_Sa_Calobra.jpg?v=1763215165&width=2400');
    background-size: cover;
    background-position: center;
    z-index: 1;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    overflow: hidden;
    box-sizing: border-box;
    transform: scale(1);
