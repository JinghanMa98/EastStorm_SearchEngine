    /* Mobile Responsive */
    @media (max-width: 968px) {
      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries {
        padding-top: 3rem;
        padding-bottom: 3rem;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-wrapper {
        max-width: 100%;
        padding: 0;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-heading {
        font-size: 1.5rem;
        margin-bottom: 2rem;
        padding: 0 1.5rem;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-controls {
        display: none;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-carousel-container {
        overflow: visible;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-carousel {
        overflow-x: auto;
        overflow-y: hidden;
        -webkit-overflow-scrolling: touch;
        scrollbar-width: none;
        -ms-overflow-style: none;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-carousel::-webkit-scrollbar {
        display: none;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonials-track {
        gap: 1rem;
        padding-left: 1.5rem;
        padding-right: 1.5rem;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonial-card {
        width: 85vw;
        max-width: 400px;
        padding: 2rem;
        min-height: 240px;
      }
    }

    @media (max-width: 768px) {
      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonial-card {
        width: 80vw;
        padding: 1.5rem;
      }

      #rt-testimonials-template--26858486727043__rt_testimonials_all_entries .rt-testimonial-text {
        -webkit-line-clamp: 5;
        line-clamp: 5;
      }
    }
  </style>

  <script>
    (function() {
      'use strict';

      const SECTION_ID = 'template--26858486727043__rt_testimonials_all_entries';
      const MOBILE_BREAKPOINT = 968;
      const DESKTOP_GAP = 32; // 2rem
      const MOBILE_GAP = 16; // 1rem
      const TRANSITION_DURATION = 600; // ms

      class TestimonialCarousel {
        constructor() {
          this.section = document.querySelector(`#rt-testimonials-${SECTION_ID}`);
          if (!this.section) return;

          this.carousel = this.section.querySelector(`[data-testimonials-carousel="${SECTION_ID}"]`);
          this.track = this.carousel.querySelector('.rt-testimonials-track');
          this.prevBtn = this.section.querySelector('.rt-testimonials-prev');
          this.nextBtn = this.section.querySelector('.rt-testimonials-next');

          // Get original cards (not clones)
          this.originalCards = Array.from(this.track.querySelectorAll('.rt-testimonial-card:not([data-clone])'));
          if (this.originalCards.length === 0) return;

          this.currentIndex = 0;
          this.isTransitioning = false;
          this.isMobile = window.innerWidth <= MOBILE_BREAKPOINT;
          this.resizeTimer = null;
          this.scrollHandler = null;

          this.init();
        }

        init() {
          this.cleanup();

          if (this.isMobile) {
            this.initMobile();
          } else {
            this.initDesktop();
          }

          this.bindResize();
        }

        cleanup() {
          // Remove all clones
          this.track.querySelectorAll('[data-clone]').forEach(el => el.remove());

          // Remove event listeners
          if (this.prevBtn) {
            this.prevBtn.replaceWith(this.prevBtn.cloneNode(true));
            this.prevBtn = this.section.querySelector('.rt-testimonials-prev');
          }
          if (this.nextBtn) {
            this.nextBtn.replaceWith(this.nextBtn.cloneNode(true));
            this.nextBtn = this.section.querySelector('.rt-testimonials-next');
          }

          // Remove scroll listener
          if (this.scrollHandler) {
            this.carousel.removeEventListener('scroll', this.scrollHandler);
            this.scrollHandler = null;
          }

          // Reset transforms
          this.track.style.transform = '';
          this.track.classList.remove('no-transition');
          this.carousel.scrollLeft = 0;
        }

        // ============ DESKTOP IMPLEMENTATION ============
        initDesktop() {
          // Clone all cards three times (before, original, after) for seamless infinite loop
          const beforeClones = this.originalCards.map(card => {
            const clone = card.cloneNode(true);
            clone.setAttribute('data-clone', 'before');
            return clone;
          });

          const afterClones = this.originalCards.map(card => {
            const clone = card.cloneNode(true);
            clone.setAttribute('data-clone', 'after');
            return clone;
          });

          // Insert clones
          beforeClones.reverse().forEach(clone => {
            this.track.insertBefore(clone, this.track.firstChild);
          });
          afterClones.forEach(clone => {
            this.track.appendChild(clone);
          });

          // Start at first real card (after before-clones)
          this.currentIndex = this.originalCards.length;
          this.allCards = Array.from(this.track.children);

          // Set initial position without animation
          this.updatePosition(true);

          // Bind button events
          this.prevBtn.addEventListener('click', () => this.navigate('prev'));
          this.nextBtn.addEventListener('click', () => this.navigate('next'));
        }

        navigate(direction) {
          if (this.isTransitioning) return;

          this.isTransitioning = true;

          if (direction === 'next') {
            this.currentIndex++;
          } else {
            this.currentIndex--;
          }

          this.updatePosition();

          setTimeout(() => {
            this.checkBoundaries();
            this.isTransitioning = false;
          }, TRANSITION_DURATION);
        }

        checkBoundaries() {
          const totalCards = this.allCards.length;
          const originalCount = this.originalCards.length;

          // If we're in the after-clones section, jump to real cards
          if (this.currentIndex >= originalCount * 2) {
            this.currentIndex = this.currentIndex - originalCount;
            this.updatePosition(true);
          }
          // If we're in the before-clones section, jump to real cards
          else if (this.currentIndex < originalCount) {
            this.currentIndex = this.currentIndex + originalCount;
            this.updatePosition(true);
          }
        }

        updatePosition(instant = false) {
          if (instant) {
            this.track.classList.add('no-transition');
          } else {
            this.track.classList.remove('no-transition');
          }

          const cardWidth = this.allCards[0].offsetWidth;
          const offset = this.currentIndex * (cardWidth + DESKTOP_GAP);
          this.track.style.transform = `translateX(-${offset}px)`;

          if (instant) {
            // Force reflow
            void this.track.offsetHeight;
            setTimeout(() => this.track.classList.remove('no-transition'), 50);
          }
        }

        // ============ MOBILE IMPLEMENTATION ============
        initMobile() {
          // Triple the cards for infinite scroll
          const cloneSet1 = this.originalCards.map(card => {
            const clone = card.cloneNode(true);
            clone.setAttribute('data-clone', 'mobile-1');
            return clone;
          });

          const cloneSet2 = this.originalCards.map(card => {
            const clone = card.cloneNode(true);
            clone.setAttribute('data-clone', 'mobile-2');
            return clone;
          });

          cloneSet1.forEach(clone => this.track.appendChild(clone));
          cloneSet2.forEach(clone => this.track.appendChild(clone));

          // Set up infinite scroll
          this.setupInfiniteScroll();
        }

        setupInfiniteScroll() {
          const originalWidth = this.calculateSetWidth();
          let isResetting = false;

          this.scrollHandler = () => {
            if (isResetting) return;

            const scrollLeft = this.carousel.scrollLeft;
            const maxScroll = originalWidth * 2; // Two sets of clones

            if (scrollLeft >= maxScroll - 10) {
              isResetting = true;
              this.carousel.scrollLeft = scrollLeft - originalWidth;
              setTimeout(() => { isResetting = false; }, 50);
            } else if (scrollLeft <= 10) {
              isResetting = true;
              this.carousel.scrollLeft = scrollLeft + originalWidth;
              setTimeout(() => { isResetting = false; }, 50);
            }
          };

          this.carousel.addEventListener('scroll', this.scrollHandler, { passive: true });

          // Start in the middle (original set)
          this.carousel.scrollLeft = originalWidth;
        }

        calculateSetWidth() {
          let width = 0;
          this.originalCards.forEach(card => {
            width += card.offsetWidth + MOBILE_GAP;
          });
          return width;
        }

        // ============ RESIZE HANDLING ============
        bindResize() {
          window.addEventListener('resize', () => {
            clearTimeout(this.resizeTimer);
            this.resizeTimer = setTimeout(() => {
              const wasMobile = this.isMobile;
              this.isMobile = window.innerWidth <= MOBILE_BREAKPOINT;

              if (wasMobile !== this.isMobile) {
                this.init();
              } else if (!this.isMobile) {
                // Just update position on desktop resize
                this.updatePosition(true);
              }
            }, 250);
          });
        }

        destroy() {
          this.cleanup();
          clearTimeout(this.resizeTimer);
        }
      }

      // Initialize carousel
      let carouselInstance = null;

      function initCarousel() {
        if (carouselInstance) {
          carouselInstance.destroy();
        }
        carouselInstance = new TestimonialCarousel();
      }

      // Initial load
      if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', initCarousel);
      } else {
        initCarousel();
      }

      // Shopify theme editor support
      document.addEventListener('shopify:section:load', function(event) {
        if (event.detail && event.detail.sectionId === SECTION_ID) {
          setTimeout(initCarousel, 100);
        }
      });
    })();
  </script>
</section>


</div><div id="shopify-section-template--26858486727043__rt_footer_9M6VCG" class="shopify-section">

