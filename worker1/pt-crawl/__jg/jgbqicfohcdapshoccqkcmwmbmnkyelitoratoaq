<script>
  (function() {
    function initRtHeroCarousel(sectionId) {
      var section = document.getElementById('rt-hero-' + sectionId);
      if (!section) return;

      if (section.rtHeroInterval) {
        clearInterval(section.rtHeroInterval);
        section.rtHeroInterval = null;
      }

      var slides = section.querySelectorAll('.rt-hero-slide');
      var dots = section.querySelectorAll('.rt-hero-dot');
      if (!slides.length) return;

      var currentIndex = 0;
      var enableAutoplay = true;
      var duration = 5000;

      function showSlide(index) {
        slides.forEach(function(slide, i) {
          slide.classList.toggle('is-active', i === index);
        });

        dots.forEach(function(dot, i) {
          dot.classList.toggle('is-active', i === index);
          var progress = dot.querySelector('.rt-hero-dot-progress');
          if (progress) {
            progress.style.animation = 'none';
            progress.style.width = '0%';
            void progress.offsetWidth;

            if (i === index && enableAutoplay && slides.length > 1) {
              progress.style.animation =
                'rtHeroProgress ' + duration + 'ms linear forwards';
            }
          }
        });

        currentIndex = index;
      }

      function nextSlide() {
        var next = (currentIndex + 1) % slides.length;
        showSlide(next);
      }

      function startAutoplay() {
        if (!enableAutoplay || slides.length <= 1) return;
        section.rtHeroInterval = setInterval(nextSlide, duration);
      }

      function resetAutoplay() {
        if (section.rtHeroInterval) {
          clearInterval(section.rtHeroInterval);
        }
        startAutoplay();
      }

      dots.forEach(function(dot, index) {
        dot.addEventListener('click', function() {
          showSlide(index);
          resetAutoplay();
        });
      });

      // Handle modal trigger buttons
      var menuButtons = section.querySelectorAll('.rt-hero-menu__btn');
      menuButtons.forEach(function(button) {
        var href = button.getAttribute('href');
        if (href && href.startsWith('#modal')) {
          button.addEventListener('click', function(e) {
            e.preventDefault();
            // Find any newsletter modal on the page
            var modal = document.querySelector('.rt-newsletter-modal');
            if (modal) {
              modal.classList.add('active');
              document.body.style.overflow = 'hidden';
            }
          });
        }
      });

      showSlide(0);
      startAutoplay();
    }

    document.addEventListener('DOMContentLoaded', function() {
      initRtHeroCarousel('template--26858486727043__rt_hero_carousel_wyE9ip');
    });

    document.addEventListener('shopify:section:load', function(event) {
      if (event.detail && event.detail.sectionId === 'template--26858486727043__rt_hero_carousel_wyE9ip') {
        initRtHeroCarousel('template--26858486727043__rt_hero_carousel_wyE9ip');
      }
    });
  })();
</script>

