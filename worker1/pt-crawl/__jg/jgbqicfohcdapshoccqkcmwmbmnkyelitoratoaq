  }

  #rt-footer-template--26858486727043__rt_footer_9M6VCG .rt-footer-background::after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(to top, rgba(29, 29, 29, 0.8), transparent);
    z-index title 14 Rouleur Travel contentType 9 text/html url 47 https://www.rouleur.cc:443/pages/rouleur-travel responseCode 3 200 