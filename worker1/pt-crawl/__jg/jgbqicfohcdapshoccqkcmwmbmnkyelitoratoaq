  <script>
    (function() {
      const sectionId = 'template--26858486727043__rt_newsletter_subscribe_xEjcLK';
      const modal = document.getElementById('rt-newsletter-modal-' + sectionId);
      const openBtn = document.getElementById('rt-subscribe-btn-' + sectionId);
      const overlay = modal?.querySelector('.rt-newsletter-modal-overlay');
      const form = document.getElementById('rt-newsletter-form-' + sectionId);
      const iframe = document.getElementById('rt-klaviyo-iframe-' + sectionId);
      const successMessage = modal?.querySelector('.rt-newsletter-success-message');
      const errorMessage = modal?.querySelector('.rt-newsletter-error-message');
      const selectFields = modal?.querySelectorAll('.rt-newsletter-select');

      if (!modal) return;

      // Handle select label floating (same as footer)
      if (selectFields) {
        selectFields.forEach(function(select) {
          var label = select.nextElementSibling;

          select.addEventListener('change', function() {
            if (this.value && this.value !== '') {
              this.classList.add('has-value');
              if (label) label.classList.add('floated');
            } else {
              this.classList.remove('has-value');
              if (label) label.classList.remove('floated');
            }
          });
        });
      }

      // Open modal (only if button exists)
      if (openBtn) {
        openBtn.addEventListener('click', function() {
          modal.classList.add('active');
          document.body.style.overflow = 'hidden';
        });
      }

      // Close modal function
      function closeModal() {
        modal.classList.remove('active');
        document.body.style.overflow = '';
        if (form) form.reset();
        if (successMessage) successMessage.style.display = 'none';
        if (errorMessage) errorMessage.style.display = 'none';

        // Reset select states
        if (selectFields) {
          selectFields.forEach(function(select) {
            select.classList.remove('has-value');
            const label = select.nextElementSibling;
            if (label) label.classList.remove('floated');
          });
        }
      }

      // Close on overlay click
      if (overlay) {
        overlay.addEventListener('click', closeModal);
      }

      // Close on escape key
      document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape' && modal.classList.contains('active')) {
          closeModal();
        }
      });

      // Handle form submission
      if (form && iframe) {
        let formSubmitted = false;

        form.addEventListener('submit', function(e) {
          // Hide any previous messages
          if (successMessage) successMessage.style.display = 'none';
          if (errorMessage) errorMessage.style.display = 'none';

          // Mark form as submitted
          formSubmitted = true;

          // Form will submit normally to Klaviyo with all fields including custom Travel field
        });

        // Listen for iframe load (indicates Klaviyo processed the form)
        let loadCount = 0;
        iframe.addEventListener('load', function() {
          loadCount++;

          // First load is just the empty iframe creation, ignore it
          if (loadCount === 1) return;

          // Only show success if form was actually submitted
          if (formSubmitted) {
            // Show success message
            if (successMessage) successMessage.style.display = 'block';
            if (errorMessage) errorMessage.style.display = 'none';

            // Close modal after 3 seconds
            setTimeout(closeModal, 3000);
          }
        });
      }
    })();
  </script>
</div>


