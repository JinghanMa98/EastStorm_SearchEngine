  <script>
    (function() {
      function initRtTripsFilters(sectionId) {
        var section = document.getElementById('rt-featured-trips-' + sectionId);
        if (!section) return;

        var buttons = section.querySelectorAll('[data-rt-trips-filter]');
        var cards = section.querySelectorAll('[data-rt-trip-card]');
        if (!buttons.length || !cards.length) return;

        var FADE_DURATION = 300;
        var STAGGER_DELAY_SECONDS = 0.1;

        function applyFilter(filter) {
          buttons.forEach(function(b) {
            var key = b.getAttribute('data-rt-trips-filter') || 'all';
            b.classList.toggle('is-active', key === filter);
          });

          // Fade out currently visible cards (matching the calendar behavior)
          cards.forEach(function(card) {
            if (card.style.display !== 'none') {
              card.classList.remove('entering');
              card.style.opacity = '0';
              card.style.transform = 'translateY(20px) scale(0.95)';
            }
          });

          setTimeout(function() {
            var visibleIndex = 0;

            cards.forEach(function(card) {
              var cats = (card.getAttribute('data-rt-trip-categories') || '').trim().split(/\s+/);
              if (!cats[0]) cats = [];
              var show = filter === 'all' || cats.indexOf(filter) !== -1;

              card.classList.remove('entering');
              card.style.animationDelay = '';

              if (show) {
                card.style.display = '';

                // Ensure the card starts from the hidden state before the reveal animation
                card.style.opacity = '0';
                card.style.transform = 'translateY(20px) scale(0.95)';

                // Force reflow so the browser applies the initial state
                void card.offsetWidth;

                card.style.animationDelay =
                  (visibleIndex * STAGGER_DELAY_SECONDS) + 's';
                card.classList.add('entering');

                visibleIndex++;
              } else {
                card.style.display = 'none';
                card.style.opacity = '';
                card.style.transform = '';
              }
            });
          }, FADE_DURATION);
        }

        buttons.forEach(function(btn) {
          btn.addEventListener('click', function() {
            var filter = btn.getAttribute('data-rt-trips-filter') || 'all';
            applyFilter(filter);
          });
        });

        applyFilter('all');
      }

      document.addEventListener('DOMContentLoaded', function() {
        initRtTripsFilters('template--26858486727043__rt_featured_trips_9a3YpN');
      });

      document.addEventListener('shopify:section:load', function(event) {
        if (event.detail && event.detail.sectionId === 'template--26858486727043__rt_featured_trips_9a3YpN') {
          initRtTripsFilters('template--26858486727043__rt_featured_trips_9a3YpN');
        }
      });
    })();
  </script>
</section>


