    //_gaq.push(['_trackEvent', 'Navigation', page_name, link_name, , true]);
  };
  for (var i = 0; i < top_menu_navigation.length; i++) {
    top_menu_navigation[i].addEventListener('click', top_menu_ga_track, false);
  }

