  }

  
  if(window.cookieStore) {
    let globalConsentCookie;

    // 0 delay setTimeout is used to ensure the following code runs after the OneTrust have fully loaded 
    // and is no longer modifying cookies
    setTimeout(async () => {
      // Remove global OptanonConsent and store its value for later creation of host-only cookie
      globalConsentCookie = await window.cookieStore.get('OptanonConsent');
      await window.cookieStore.delete({ name: 'OptanonConsent', domain: 'yougov.com' });

      // Remove global OptanonAlertBoxClosed and replace it with host-only cookie
      const globalClosedCookie = await window.cookieStore.get('OptanonAlertBoxClosed');
      await window.cookieStore.delete({ name: 'OptanonAlertBoxClosed', domain: 'yougov.com' });
      globalClosedCookie && await window.cookieStore.set({ ...globalClosedCookie, domain: undefined });
    });

    // After any OT consent change
    OneTrust.OnConsentChanged(async (event) => { 
      // If global consent cookie is not stored yet, do nothing
      if(!globalConsentCookie) {
        return;
      }

      // Get updated consent groups in a proper cookie format
      const changedConsentGroups = ['C0001', 'C0002', 'C0003', 'C0004'] 
        .map((code) => `${code}:${event.detail.includes(code) ? '1' : '0'}`)
        .join(',');
      const encodedChangedConsentGroups = encodeURIComponent(changedConsentGroups);
      const consentCookieValue = globalConsentCookie.value.replace(/(&groups=)[^&]*/g, `$1${encodedChangedConsentGroups}`);

      // Save host-only consent cookie for later OptanonConsent restoration after page reload
      await window.cookieStore.set({
        name: 'OptanonConsentHostOnly',
        value: consentCookieValue,
        domain: undefined,
        expires: globalConsentCookie.expires,
      });
    });
