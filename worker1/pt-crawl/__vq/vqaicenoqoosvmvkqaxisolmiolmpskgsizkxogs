            srcset="https://static0.colliderimages.com/wordpress/wp-content/uploads/2022/12/michele-morrone-in-the-next-365-days-1.jpg?q=50&amp;fit=crop&amp;w=800&amp;dpr=1.5"
                    />
        <source
            media="(max-width: 1023px)"
            data-srcset="https://static0.colliderimages.com/wordpress/wp-content/uploads/2022/12/michele-morrone-in-the-next-365-days-1.jpg?q=50&amp;fit=crop&amp;w=825&amp;dpr=1.5"
            srcset="https://static0.colliderimages.com/wordpress/wp-content/uploads/2022/12/michele-morrone-in-the-next-365-days-1.jpg?q=50&amp;fit=crop&amp;w=825&amp;dpr=1.5"
