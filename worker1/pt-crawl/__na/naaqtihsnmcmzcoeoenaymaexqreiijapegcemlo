var _shiftClay2 = _interopRequireDefault(_shiftClay);

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

// load logo styles
__webpack_require__(261);

(0, _shiftClay2.default)();

