 reality\",\"link\":\"https:\\/\\/time.com\\/collections\\/100-best-podcasts\\/7303109\\/the-promise\",\"editLink\":\"https:\\/\\/api.time.com\\/wp-admin\\/post.php?post=7303109\u0026action=edit\",\"postStatus\":\"publish\",\"featuredMediaUrl\":\"https:\\/\\/api.time.com\\/wp-content\\/uploads\\/2025\\/07\\/Best-Podcasts-2025-The-Promise-070.jpg?quality=85\u0026w=600\u0026h=600\u0026crop=1\",\"slug\":\"the-promise\",\"shortTitle\":\"The Promise\",\"brandedURL\":\"\",\"authorsData\":[{\"types\":[\"contributor\"],\"id\":634871034,\"path\":\"\\/author\\/jake-kring-schreifels\\/\",\"name\":\"Jake Kring-Schreifels\",\"bio\":\"King-Schreifels is a contributor for TIME.\",\"twitter\":\"\",\"email\":\"\",\"firstname\":\"Jake\",\"lastname\":\"King-Schreifels\",\"title\":\"King-Schreifels is a contributor for TIME.\",\"facebook\":\"\",\"google\":\"\",\"instagram\":\"\",\"linkedin\":\"\",\"thumbnail\":\"\"}],\"tagsData\":[{\"id\":634871763,\"name\":\"Society\",\"slug\":\"society\",\"link\":\"\\/collection_tag\\/society\"}]},{\"id\":7303110,\"title\":\"Radiolab\",\"secondary_title\":\"Curiosity about the world around us\",\"link\":\"https:\\/\\/time.com\\/collections\\/100-best-podcasts\\/7303110\\/radiolab\",\"editLink\":\"https:\\/\\/api.time.com\\/wp-admin\\/post.php?post=7303110\u0026action=edit\",\"postStatus\":\"publish\",\"featuredMediaUrl\":\"https:\\/\\/api.time.com\\/wp-content\\/uploads\\/2025\\/07\\/Best-Podcasts-2025-Radiolab-071.jpg?quality=85\u0026w=600\u0026h=600\u0026crop=1\",\"slug\":\"radiolab\",\"shortTitle\":\"Radiolab\",\"brandedURL\":\"\",\"authorsData\":[{\"types\":[\"time staff\"],\"id\":634870768,\"path\":\"\\/author\\/rebecca-schneid\\/\",\"name\":\"Rebecca Schneid\",\"bio\":\"\u003cdiv class=\\\"c-message_kit__blocks c-message_kit__blocks--rich_text\\\"\u003e\\r\\n\u003cdiv class=\\\"c-message__message_blocks c-message__message_blocks--rich_text\\\" data-qa=\\\"message-text\\\"\u003e\\r\\n\u003cdiv class=\\\"p-block_kit_renderer\\\" data-qa=\\\"block