        </div>
    </div>
</script>

<script>
    window.onload = function () {
        var remoteDataSource = new kendo.data.DataSource({
            type: "odata-v4",
            transport: {
                read: "/api/news/newsitems?sf_provider=newsProvider16&sf_culture=en&$top=3&$orderby=PublicationDateAndTime%20desc&$select=Title,ItemDefaultUrl,FormatedDate,NewsType"
            }
        });
        $("#listView").kendoListView({
            dataSource: remoteDataSource,
            pageable: true,
            template: kendo.template($("#template").html()),
            dataBound: function () {
                this.wrapper.removeClass("k-widget");
                this.wrapper.removeClass("k-listview");
            }
        });
    };
</script>

