                                                          $(document).ready(function() {
                                                              var $fts = $('#ftsHeader');
                                                              var sidebarFTSCheckbox = document.getElementById('fts');
                                                              var ariaLabelText = 'Search within full text checkbox';
                                                              $('#ftsCheckbox').on('change', function(e) {
                                                                  $fts.val(e.target.checked ? 'yes' : 'no');
