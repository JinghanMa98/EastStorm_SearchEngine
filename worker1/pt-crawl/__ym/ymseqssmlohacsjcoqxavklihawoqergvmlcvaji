    <div class="layout-wrapper">
        <div class="waf-body">
            <div class="preview-swiper swiper" id="aboutus">
                <div class="article-list swiper-wrapper">
                    <article class="article-item swiper-slide">
                        <div class="article-wrap">
                            <div class="article-thumbnail">
                                <a href="https://www.paralympic.org/paralympic-games">
