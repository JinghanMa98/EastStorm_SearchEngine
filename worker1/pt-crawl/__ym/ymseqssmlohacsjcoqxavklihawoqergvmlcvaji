	 <script>
		  // JavaScript countdown timer
		  		    		      // Set the target date to the start date of the event in UTC
		      var targetDate1 = Date.UTC('2026', '2', '06', 18, 0, 0);
		    		    // Update the countdown every second
		    var x1 = setInterval(function() {
		      // Get the current date and time in UTC
		      var now = new Date().getTime();
		
		      // Calculate the time remaining
		      var distance = targetDate1 - now;
		
		      // Calculate days, hours, minutes, and seconds remaining
		      var days = Math.floor(distance / (1000 * 60 * 60 * 24));
		      var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
		      var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
		      var seconds = Math.floor((distance % (1000 * 60)) / 1000);
		
		      // Display the countdown
		      document.getElementById("countdown-1").innerHTML = `
		        <div class="meta">
		          <span class="meta-value">${days}</span>
		          <span class="meta-label">Days</span>
		        </div>
		        <div class="meta">
		          <span class="meta-value">${hours}</span>
		          <span class="meta-label">Hours</span>
		        </div>
		        <div class="meta">
		          <span class="meta-value">${minutes}</span>
		          <span class="meta-label">Minutes</span>
		        </div>
		        <div class="meta">
		          <span class="meta-value">${seconds}</span>
		          <span class="meta-label">Seconds</span>
		        </div>
		      `;
		
		      // If the countdown is over, display '0' and clear the interval
		      if (distance < 0) {
		        clearInterval(x1);
		        document.getElementById("countdown-1").innerHTML = `
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Days</span>
		          </div>
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Hours</span>
		          </div>
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Minutes</span>
		          </div>
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Seconds</span>
		          </div>
		        `;
		      }
		    }, 1000);
		  		    		      // Set the target date to the start date of the event in UTC
		      var targetDate2 = Date.UTC('2028', '7', '15', 18, 0, 0);
		    		    // Update the countdown every second
		    var x2 = setInterval(function() {
		      // Get the current date and time in UTC
		      var now = new Date().getTime();
		
		      // Calculate the time remaining
		      var distance = targetDate2 - now;
		
		      // Calculate days, hours, minutes, and seconds remaining
		      var days = Math.floor(distance / (1000 * 60 * 60 * 24));
		      var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
		      var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
		      var seconds = Math.floor((distance % (1000 * 60)) / 1000);
		
		      // Display the countdown
		      document.getElementById("countdown-2").innerHTML = `
		        <div class="meta">
		          <span class="meta-value">${days}</span>
		          <span class="meta-label">Days</span>
		        </div>
		        <div class="meta">
		          <span class="meta-value">${hours}</span>
		          <span class="meta-label">Hours</span>
		        </div>
		        <div class="meta">
		          <span class="meta-value">${minutes}</span>
		          <span class="meta-label">Minutes</span>
		        </div>
		        <div class="meta">
		          <span class="meta-value">${seconds}</span>
		          <span class="meta-label">Seconds</span>
		        </div>
		      `;
		
		      // If the countdown is over, display '0' and clear the interval
		      if (distance < 0) {
		        clearInterval(x2);
		        document.getElementById("countdown-2").innerHTML = `
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Days</span>
		          </div>
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Hours</span>
		          </div>
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Minutes</span>
		          </div>
		          <div class="meta">
		            <span class="meta-value">0</span>
		            <span class="meta-label">Seconds</span>
		          </div>
		        `;
		      }
		    }, 1000);
		  		</script>
