              // downscroll code
              body.classList.add("beijing-2022-scroll");
           } 
           if(st_ == 0){
              // upscroll code
              body.classList.remove("beijing-2022-scroll");
           }
           lastScrollTopBeijing = st_ <= 0 ? 0 : st_; // For Mobile or negative scrolling
        }, false);
