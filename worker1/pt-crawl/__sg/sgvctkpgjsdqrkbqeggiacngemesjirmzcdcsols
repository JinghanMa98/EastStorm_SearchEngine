                  <script type="text/javascript">
                    function closeShareaholicPopup(isMobile) {
                      try {
                        $(isMobile ? '#close-icon-share-responsive' : '#close-icon-share').focus();
                        var allSocialShareOptionsByClass = $('.social-share-options');
                        allSocialShareOptionsByClass.hide();
                      } catch (err) {
                        console.error('Error while hiding Shareaholic layer:', err.message, err.stack);
                      }
                    }
                
                    function setFocusTrap(isMobile) {
                      AOP.focusTrap(isMobile ? 'share-icons-modal-responsive' : 'share-icons-modal');
                    }
                
                    $(document).ready(function() {
                      $('#close-icon-share, #close-icon-share-responsive').on('click', function() {
                        var content = $('#share-content');
                        var isExpanded = this.getAttribute('aria-expanded') === 'true';
                        this.setAttribute('aria-expanded', !isExpanded);
                        content.css('display', isExpanded ? 'none' : 'block');
                      });
                    });
                  </script>
                
