                      $(document).ready(function () {
                        $('body').on('click', '#productCitations .section-button', function (e) {
                          e.preventDefault();
                          var id = $(this).attr('data-id');
                          $('#productCitations .section-content section').hide();
                          $('#productCitations .section-container').removeClass('active');
                          $(this).parent('.section-container').addClass('active');
                          $('#' + id).show();
