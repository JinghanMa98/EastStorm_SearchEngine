                  </ul>
                  <span class="toggle-authors-btn" role="button" tabindex="0">Show more authors <img
                    src="/core/cambridge-core/public/images/dropdown_arrow_black.svg" alt="" class="toggle-arrow"
                    onerror="this.style.display='none'" /></span>
                </div>
                
                <script>
                  $(function() {
                    var AUTHOR_LIMIT = 6;
                
                    function initializeAllAuthorToggles() {
                      $('.toggle-authors-btn').each(function() {
                        var $btn = $(this);
                        if (!$btn.data('initialized')) {
                          initializeAuthorToggle($btn);
                        }
                      });
                    }
                
                    function initializeAuthorToggle($btn) {
                      var $container = $btn.closest('div');
                      var $truncatedContent = $container.find('.authors-content-truncated');
                      var $fullContent = $container.find('.authors-content-full');
                
                      if (!$truncatedContent.length || !$fullContent.length) return;
                
                      $btn.data('initialized', true);
                
                      var $authorLinks = $truncatedContent.find('a.more-by-this-author');
                      if (!$authorLinks.length) {
                        $authorLinks = $fullContent.find('a.more-by-this-author');
                      }
                
                      var totalAuthors = $authorLinks.length;
                      $btn.data('total-authors', totalAuthors);
                
                      if (totalAuthors < AUTHOR_LIMIT) {
                        $btn.hide();
                        $truncatedContent.show();
                        $fullContent.hide();
                        return;
                      }
                
                      if (!$truncatedContent.data('original-stored')) {
                        $truncatedContent.data({
                          'original-content': $truncatedContent.html(),
                          'original-stored': true
                        });
                      }
                
                      showTruncatedView($truncatedContent, $fullContent, $btn, $authorLinks);
                      setupEventHandlers($btn, $truncatedContent, $fullContent);
                    }
                
                    function setupEventHandlers($btn, $truncatedContent, $fullContent) {
                      $btn.off('click keydown').on('click keydown', function(e) {
                        if (e.type === 'click' || (e.type === 'keydown' && (e.key === 'Enter' || e.key === ' '))) {
                          e.preventDefault();
                          showFullView($truncatedContent, $fullContent, $btn);
                        }
                      });
                    }
                
                    function showTruncatedView($truncatedContent, $fullContent, $btn, $authorLinks) {
                      var totalAuthors = $btn.data('total-authors') || $authorLinks.length;
                
                      var originalContent = $truncatedContent.data('original-content');
                      if (originalContent) {
                        $truncatedContent.html(originalContent);
                        $authorLinks = $truncatedContent.find('a.more-by-this-author');
                      }
                
                      if (totalAuthors < AUTHOR_LIMIT) {
                        $truncatedContent.show();
                        $fullContent.hide();
                        $btn.hide();
                        return;
                      }
                
                      var $authorContainers = $truncatedContent.find('li.author');
                      var authorCount = 0;
                      var shouldTruncate = false;
                
                      $authorContainers.each(function(containerIndex) {
                        var $container = $(this);
                        var $authorsInContainer = $container.find('a.more-by-this-author');
                
                        if (authorCount >= AUTHOR_LIMIT) {
                          $container.hide();
                          shouldTruncate = true;
                          return;
                        }
                
                        var authorsToShow = Math.min(AUTHOR_LIMIT - authorCount, $authorsInContainer.length);
                
                        if (authorsToShow < $authorsInContainer.length) {
                          // Need to truncate this container
                          var truncatedHtml = buildTruncatedContainerHtml($container, authorsToShow, totalAuthors, authorCount + authorsToShow >= AUTHOR_LIMIT);
                          $container.replaceWith(truncatedHtml);
                          shouldTruncate = true;
                        }
                
                        authorCount += authorsToShow;
                
                        if (authorCount >= AUTHOR_LIMIT) {
                          // Hide remaining containers
                          $authorContainers.slice(containerIndex + 1).hide();
                          shouldTruncate = true;
                        }
                      });
                
                      $truncatedContent.show();
                      $fullContent.hide();
                      $btn.show();
                    }
                
                    function buildTruncatedContainerHtml($container, authorsToShow, totalAuthors, addEllipsis) {
                      let truncatedHtml = '';
                      let authorTypeLabel = '';
                
                      const $containerDiv = $container.find('div').first();
                      const containerContents = $containerDiv.length ? $containerDiv.contents() : $container.contents();
                
                      for (let i = 0; i < containerContents.length; i++) {
                        const node = containerContents[i];
                        if (node.nodeType === Node.TEXT_NODE) {
                          const text = node.textContent.trim();
                          if (text && !authorTypeLabel) {
                            authorTypeLabel = text;
                            break;
                          }
                        } else if ($(node).is('a.more-by-this-author')) {
                          break;
                        }
                      }
                
                      if ($containerDiv.length) {
                        truncatedHtml = '<li class="author"><div>';
                        if (authorTypeLabel) truncatedHtml += authorTypeLabel + ' ';
                      } else {
                        truncatedHtml = '<li class="author">';
                        if (authorTypeLabel) truncatedHtml += authorTypeLabel + ' ';
                      }
                
                      var $authorLinks = $container.find('a.more-by-this-author');
                
                      for (let index = 0; index < authorsToShow && index < $authorLinks.length; index++) {
                        const $author = $($authorLinks[index]);
                        const authorLinkHtml = $author.prop('outerHTML');
                
                        let affiliationHtml = '';
                        let $next = $author.next();
                
                        if ($next.length && $next.text().trim() === ',') {
                          $next = $next.next();
                        }
                
                        if ($next.length && $next.hasClass('affiliation')) {
                          affiliationHtml = ', ' + $next.prop('outerHTML');
                        }
                
                        truncatedHtml += authorLinkHtml + affiliationHtml;
                
                        if (index < authorsToShow - 1) {
                          truncatedHtml += ', ';
                        }
                      }
                
                      if (addEllipsis && totalAuthors > AUTHOR_LIMIT) {
                        truncatedHtml = truncatedHtml.replace(/,\s*$/, '');
                        truncatedHtml += '...';
                      }
                
                      truncatedHtml += $containerDiv.length ? '</div></li>' : '</li>';
                
                      return truncatedHtml;
                    }
                
                    function showFullView($truncatedContent, $fullContent, $btn) {
                      $truncatedContent.hide();
                
                      var $fullContentClone = $fullContent.clone();
                      $fullContentClone.find('.main-contributors').remove();
                      $fullContent.html($fullContentClone.html());
                
                      $fullContent.show();
                      $btn.hide();
                    }
                
                    $(document).ready(function() {
                      $({}).delay(100).queue(function() {
                        initializeAllAuthorToggles();
                        $(this).dequeue();
                      });
                    });
                
                    $(window).on('load', function() {
                      $({}).delay(100).queue(function() {
                        initializeAllAuthorToggles();
                        $(this).dequeue();
                      });
                    });
                
                    $('<style>').prop('type', 'text/css').html(`
                      .toggle-authors-btn {
                        cursor: pointer;
                        display: inline-block;
                        margin-top: 5px;
                        color: #0066cc;
                        text-decoration: underline;
                      }
                      .toggle-authors-btn:hover {
                        color: #0052a3;
                      }
                      .toggle-authors-btn:focus {
                        outline: 2px solid #007acc;
                        outline-offset: 2px;
                      }
                    `).appendTo('head');
                  });
                </script>  <li><div class="divider-selector-section"></div></li>
                <div class="book-ecommerce-overview-transactions-access mobile-only">
                  
                  
                   <!-- not logged in -->
                            <div>
                              <div class="book-ecommerce-overview-transactions-access access-text">You may already have access via personal or institutional login</div>
                              <div class="book-ecommerce-overview-transactions-access check-access">
                                  <li>
                                    <ul class="access-modal">
                                        <li class="status get">
                                            <a href="#"
                                               class="get hasTooltipCustom-top secondary medium"
                                               aria-label="Check access for "
                                               data-qtip-event="click"
                                               data-cup-event='{"eventCode":"TA-EX","eventContext":"/core/books/research-methods-for-science/1C04E5D747781B68C52A79EE86BF584B","usedIdentities":[],"authenticationMethods":[],"referringURL":"","eventType":"GET","searchScope":"SITE"}'
                                              >Check access</a>
                                          <div class="custom-tooltip custom-tooltip--buttons">
                                            <div class="grey-bg">
                                              <div class="row">
                                                <div class="column small-12 text-center tooltip-row">Check if you have access via personal or institutional login</div>
                                              </div>
                                              <div class="tooltip-row row text-center padding-bottom">
                                                  <a href="/core/login?ref=/core/books/research-methods-for-science/1C04E5D747781B68C52A79EE86BF584B" class="button small radius blue ">Log in</a>
                                                  <a href="/core/register?ref=/core/books/research-methods-for-science/1C04E5D747781B68C52A79EE86BF584B" class="button small radius blue ">Register</a>
                                              </div>
