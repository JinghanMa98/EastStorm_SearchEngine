			<script type="text/javascript">
			// Event Calendar
			// copyright Stephen Chapman, 10th June 2006, 30th October 2006
			
			// global flag
			var isIE = false;
			var months = new Array(13);
				   months[01]  = "January";
				   months[02]  = "February";
				   months[03]  = "March";
				   months[04]  = "April";
				   months[05]  = "May";
				   months[06]  = "June";
				   months[07]  = "July";
				   months[08]  = "August";
				   months[09]  = "September";
				   months[10]  = "October";
				   months[11] = "November";
				   months[12] = "December";
			// global request and XML document objects
			var req;
			
			//get th,rd, for a date
			function ordi(n){
			var s='th';
			if(n==1 || n==21 || n==31) s='st';
			if(n==2 ||n==22) s='nd';
			if(n==3 ||n==23) s='rd';
			return n+s;
			}
			//Get DAY/NAME of the day of the week
			function getTheDay(aText)
			{
				myDays= 
				["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"]
				myDate=new Date(eval('"'+aText+'"'))
			 	return myDays[myDate.getDay()]
			}
			
			
			// function builds the date to appear in Event Calendar header
			function getproperDatetodisplay(dt)
			{
			
				var DDDD=dt.toString();
				var y=DDDD.substring(0,4);
				var mo=DDDD.substring(4);
				var mo1=mo.substring(0,2);
				var monthname   = months[parseInt(mo1,10)];
				var submonth=monthname.substring(0,3);
				var d=DDDD.substring(6);
				var FinalDate=submonth+' '+ordi(d) +', '+y;
				var FinalDate1=d+' '+ monthname +', '+y;
				var DayName= getTheDay(FinalDate1);
				
				if (d =='08')
				{var newd='8';}
				else if (d =='09')
				{var newd='9';}
				else
				{var newd=parseInt(d);}
				
				monthabbr = ". ";
				if (monthname.length <= 3){
					monthabbr = ' ';
				}
				
				FinalDateToReturn=DayName+' ' +submonth+monthabbr+newd +', '+y;

				return FinalDateToReturn;
					
				
			}
			
			// deletes events calendar and rebuilds the table
			function DeleteTable() {    
			    var table = document.getElementById('dayCal'); 
			    if (!table) { return; }
			    if (table.parentNode != null) {
				  pNode = table.parentNode;
				  pNode.removeChild (table);
				  return;
				} 
			    if (table != null)
			    {  
			        var rows = table.rows;     
			        while(rows.length){ // length=0 -> stop
			       	table.deleteRow(rows.length-1); 
			        }
			    }
			}
			// this will remove whitespace in FIREFOX otherwise, all whitespaces are counted as Nodes throwing of fthe counts
			function removeWhiteSpace(root) {
				var i = 0;
				while (i != root.childNodes.length) {
					if (root.childNodes[i].nodeType == 3) {
					root.removeChild(root.childNodes[i])
					} else {
					var j = 0;
					while (j != root.childNodes[i].childNodes.length) {
					if (root.childNodes[i].childNodes[j].nodeType == 3) {
					root.childNodes[i].removeChild(root.childNodes[i].childNodes[j])
					} else {
						j++;
						}
					}
				i++;
				}
				}
			return root;
			}
			
			// these 2 functions will SHOW or HIDE hiddent rows
			function toggle( elem ) {
			vis = elem.style;
			if(vis.display==''&&elem.offsetWidth!=undefined&&elem.offsetHeight!=undefined)
			vis.display = (elem.offsetWidth!=0&&elem.offsetHeight!=0)?'block':'none';
			vis.display = (vis.display==''||vis.display=='block')?'none':'block';
			}
			function hideans() {
			var boxes = document.getElementsByTagName("TR");
			for(var i=0;i<boxes.length;i++)
				{ 
					if(boxes[i].className.match("tr_tohide")) 
					{toggle(boxes[i]);}
				}	
			}
			
			
			function getUniqueId()
			{
			     var dateObject = new Date();
			     var uniqueId = 
			          dateObject.getFullYear() + '' + 
			          dateObject.getMonth() + '' + 
			          dateObject.getDate() + '' + 
			          dateObject.getTime();
			
			     return uniqueId;
			};
			
			
			// ==================this is month calendar functions =======================================//
			var tb = 't'; // top or bottom (t or b)
			var headbg = '#cccccc';  // table heading background colour
			var todaybg = '#99cccc'; // current selected date background colour
			var textclr = '#000000'; // text colour
			var linkclr = '#000000'; // link text colour 
			var noMessage =  'No event messages to display for today'; // message to display when no entry in array
			
			var dA = new Array(); var x = 0;
			// first 8 characters in ccyymmdd format for single date events
			// first 8 characters in 0000mmdd format for every year events
			// dA[x++] = "20060610 New Event Calendar Started today";
			// permission is granted to use this javascript provided that the below code is not altered
			var pageLoaded = 0; window.onload = function() {pageLoaded = 1;}
			function loaded(i,f) 
			{
				if (document.getElementById && document.getElementById(i) != null) f(); 
				else if (!pageLoaded) setTimeout('loaded(\''+i+'\','+f+')',100);
			}
			
			function monthlength(month,year) 
			{
				var dd = new Date(year, month, 0);return dd.getDate();
			}
			
			var moy = ['January','February','March','April','May','June','July','August','September','October','November','December'];
			var today = new Date();
			var selDate = today.getFullYear()+getmmdd(today.getMonth()+1,today.getDate());
			var mmmmm=today.getMonth()+1;
			if (mmmmm.toString().length==1) { mmmmm = '0' + mmmmm; }
			var selMonth = today.getFullYear()+''+mmmmm;
			
			
			//SMALL CALENDAR GENERATION
			function dispCal(yy,mm) 
			{
				if (mm < 0 || mm > 12) 
				{alert('month must be between 1 and 12'); return false;} 
				if (yy != 0 && (yy < 1901 || yy > 2100)) 
				{alert('year must be after 1900 and before 2101'); return false;} 
				
				var dow = ['Sun','Mon','Tue','Wed','Thu','Fri','Sat']; 
				var calendar = new Date();  
				var today = calendar.getDate(); calendar.setDate(1); 
				if (yy > 1900) calendar.setFullYear(yy); 
				if (mm > 0) calendar.setMonth(mm - 1); 
				var yy = year = calendar.getFullYear(); 
				var month = calendar.getMonth(); 
				calendar.setDate(today); 
				var weekday = calendar.getDay(); 
				var daysInMonth = monthlength(month+1,year); 
				var hilite_start = '<td class="current">'; 
				var td_start = '<td>'; 
				var td_startDOW = '<th scope="col">'; 
				var td_startweekend= '<td scope="row" class="weekend">'; 
				var td_startWeekendDOW = '<th scope="col" class="weekend">'; 
				
			
				var cal = '';
				//CALENDAR CONTROLS/NAVIGATION
				if (document.getElementById) 
				{
					var mmb = month;  mm = month + 1; 
					var yya = yyb = yy; 
					if (mmb <1) 
						{
						mmb += 12; yyb--;
						} 
					var mma = month + 2; 
					if (mma > 12) 
						{
						mma -= 12; yya++;
						} 
					var yb = yy -1; 
					var ya = yy +1; 
					
					cal += '<div class="calendar-header">'
							+'<h5>'+moy[month]+ ' ' + year+'</h5>'
							+'<ul class="calendar-nav">'
								+'<li class="previous"><a href="#" onclick="javascript:if (cala = dispCal('+yyb+','+mmb+')) {document.getElementById(\'calendar\').innerHTML = cala; return false;}">Previous</a></li>'
								+'<li class="next"><a href="#" onclick="javascript:if (cala = dispCal('+yya+','+mma+')) {document.getElementById(\'calendar\').innerHTML = cala; return false;}">Next</a></li>'
							+'</ul>';
						+'</div>';
			
				} //END: CALENDAR CONTROLS/NAVIGATION
				else {
					alert('else END: CALENDAR CONTROLS/NAVIGATION');	
					cal += '</div>';
				} 
				
				
					
				//SMALL CALENDAR TABLE STARTS	
				cal+='</div><table id="monthCal" summary="Select a day of the month to view a full list of the day\'s happenings"><thead><tr>'; 
				
					//DAYS OF THE WEEK LABELS
					for(dex=0; dex < 7; dex++) 
						if (dex!=0 && dex!=6)
						cal += td_startDOW + dow[dex] + '</th>';
						else
						cal += td_startWeekendDOW + dow[dex] + '</th>';
					cal += '<\/tr></thead><tbody><tr>'; 
					//END: DAYS OF THE WEEK LABELS
				
				
				
				var day2 = today; 
				for (dex = today; dex > 6; dex -=7) 
				day2 = dex; weekday -= day2 - 1;
				
				
					// BLANK DAYS IN THE BEGINNING OF THE CALENDAR
					while (weekday < 0) weekday += 7; 
					for(dex=0; dex < weekday; dex++) 
						if (dex == 6 || dex == 0)
						cal += td_startweekend + '&nbsp;<\/td>'; 
						else
						cal += td_start + '&nbsp;<\/td>'; 
					// END: BLANK DAYS IN THE BEGINNING OF THE CALENDAR
				
				
					//DISPLAY DAYS ON CALENDAR
					for (dex = 1; dex <= daysInMonth; dex++) {
						if (weekday == 7) {
							cal += '</tr><tr>';
							weekday = 0;
						}
						
						var monthCorrected = month+1;
						var myLink = '<a href=\"http://dyn.politico.com/blogs/media/?month=' + monthCorrected + '&year=' + year + '&day=' + dex + '\" >' + dex + '<\/a><\/td>';
						if (selDate == year + getmmdd(month + 1, dex)) {
							cal += hilite_start + myLink;
						}
						else {
							if (weekday == 6 || weekday == 0) {
								cal += td_startweekend + myLink;
							}
							else {
								cal += td_start + myLink;
							}
						}
						weekday += 1;
					} 
					//END: DISPLAY DAYS ON CALENDAR
					
					
					//BLANK DAYS IN THE END OF THE CALENDAR
					for(dex=weekday; dex < 7; dex++) 
					if (dex == 6 || dex == 0)
						cal += td_startweekend + '&nbsp;<\/td>'; 
					else
						cal += td_start + '&nbsp;<\/td>'; 
					cal += '<\/tr></tbody><\/table></div>';
					
					// END: BLANK DAYS IN THE END OF THE CALENDAR
				
				//alert(cal);
				//debug document.write(cal);
				return cal;
			}
			
			
			// this function runs on page load
			function start() 
			{
				//CALL XML LOAD PROCESS
			
				var RMonth=selMonth;
				var RDate=selDate;
				
				//loadDoc(RMonth,RDate);
				var y = ''; 
				if (tb == 't') 
					//y = x + dispCal(0,0); 
					y =dispCal(0,0);//+x; 
				else 
					y = dispCal(0,0);// + x; 
				
				document.getElementById('calendar').innerHTML = y; 
				var dateToSendForDisplay=getproperDatetodisplay(selDate);
			    //call to display TODAYS date on the main calendar by default
				//ev(dateToSendForDisplay,selDate);
			}
			
			loaded('calendar',start);
			
			function nextDate(yymmdd) 
			{
				var x = dA.length;
				for (var i = 0; i < x; i++) 
				{
					return dA[i].substr(8);
				}
				return '';
			}
			
			function getmmdd(mm,dd) 
			{
				return (mm > 9 ? '' + mm : '0' + mm) + (dd > 9 ? dd : '0' + dd);
			}
			</script>
			
					<script>
			$(document).ready(function(){ 
				//TODO: move to common function in common.js
				//$.commonWidgetsLoadDivViaXml("https://web.archive.org/web/20130719234503/http://www.politico.com/includes/xml/widgetVideo.xml");

				$.ajax({type:"GET", url:"https://web.archive.org/web/20130719234503/http://www.politico.com/includes/xml/widgetVideo.xml"+cacheBuster(300), cache: true,success: function(xml){
							var rootNode = $(xml).find('inc');
							var rootNodeID = rootNode.attr("divid");  
							var rootNodeText = rootNode.text();   
							var tempIDName="#"+rootNodeID;
							$(tempIDName).html('');
							$(tempIDName).html(rootNodeText).commonWidgetsJs();
							}
					});//ajax	
			});
			</script>
			<script>
			</script>
				</head>



