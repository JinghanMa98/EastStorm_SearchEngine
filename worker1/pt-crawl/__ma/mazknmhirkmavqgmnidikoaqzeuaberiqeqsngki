	<script type="text/javascript">
	FB.Event.subscribe('comment.create', function(response) {
		
		update_count('https://web.archive.org/web/20130719234503/http://www.politico.com/blogs/media/2012/08/time-magazine-still-on-top-in-circulation-131349.html',"comment_count");
		
	});
	function update_count(url,cls){
		FB.api(
			{
				method: 'fql.query',
				query: "select id from comment where (object_id in (select comments_fbid from link_stat where url ='"+url+"') OR object_id in (select post_fbid from comment where is_private=0 and object_id in (select comments_fbid from link_stat where url ='"+url+"'))) and is_private = 0"					   
	        },
	        function(data) {
				if(data){
		            $("span."+cls).html(data.length);
				}
	        }
		);
	}
	</script>

