        updateFontSize(currentFontSize + 1);
      }
    });
    showShare?.addEventListener("click", () => {
      const isActive = isShareActiveInput.value === "true";

      gtag("event", "click", {
        event_category: "Article Tool Bar",
        event_label: isActive ? "Close Share Options" : "Open Share Options",
        interaction_type: isActive
          ? "close share overlay"
          : "open share overlay",
