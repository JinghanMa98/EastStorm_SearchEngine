    let currentFontSize = 16;
    let defaultFontSize = 16;
    function updateFontSize(newSizePx) {
         if (!isGoldUserFlag) {
        if (promo1) {
          const isVisible = window.getComputedStyle(promo1).display === "block";
          promo1.style.display = "none";
          transparentBg.style.display = "none";
        }
        if (promo2) {
          const isVisible = window.getComputedStyle(promo2).display === "block";
          promo2.style.display = "none";
          transparentBg.style.display = "none";
        }
      }
      currentFontSize = newSizePx;
      document.documentElement.style.setProperty(
        "--user-font-size",
        `${currentFontSize}px`
      );

      if (currentFontSize > defaultFontSize) {
        fontPlusBtn.style.color = "gold";
        fontMinusBtn.style.color = "#fff";
      } else if (currentFontSize < defaultFontSize) {
        fontMinusBtn.style.color = "gold";
        fontPlusBtn.style.color = "#fff";
      } else {
        fontPlusBtn.style.color = "#fff";
        fontMinusBtn.style.color = "#fff";
      }

      const fontIconDefault = document.getElementById("fontIconDefault");
      const fontIconActive = document.getElementById("fontIconActive");
      if (currentFontSize !== defaultFontSize) {
        fontIconDefault.style.display = "none";
        fontIconActive.style.display = "block";
      } else {
        fontIconDefault.style.display = "block";
        fontIconActive.style.display = "none";
      }
    }

    document.addEventListener("DOMContentLoaded", () => {
      const overlay = document.getElementById("nudgeOverlay");
      let nudgeContainers = [
        document.querySelector(
          ".group1 > .articletoolsnudge, .group1 > *:first-child"
        ),
        document.querySelector(
          ".group2 > .articletoolsnudge, .group2 > *:first-child"
        ),
        document.querySelector(
          ".group3 > .articletoolsnudge, .group3 > *:first-child"
        ),
        document.querySelector(
          ".group4 > .articletoolsnudge, .group4 > *:first-child"
        ),
      ].filter(Boolean);

      let highlightTargets = [
        document.getElementById("showSpeech"),
        document.getElementById("showLanguages"),
        document.getElementById("showShare"),
        document.querySelector(".fontAA"),
      ];
      if (isAvail == "false") {
        highlightTargets.shift();
      }
      const localKey = "nudgeShownCount";
      const maxViews = 2;
      let viewCount = parseInt(localStorage.getItem(localKey) || "0", 10);
      if (viewCount >= maxViews) return;

      let currentIndex =  -1;

      function hideAllNudges() {
        nudgeContainers.forEach((n) => (n.style.display = "none"));
        overlay.style.display = "none";
        document.body.style.setProperty("overflow", "auto", "important");
        document.documentElement.style.setProperty(
          "overflow",
          "auto",
          "important"
        );

          document.getElementById("showSpeech")?.style.setProperty("pointer-events", "auto", "important");
  document.getElementById("showLanguages")?.style.setProperty("pointer-events", "auto", "important");
  document.getElementById("showShare")?.style.setProperty("pointer-events", "auto", "important");

  document.querySelectorAll(".fontAA").forEach(el => {
    el.style.setProperty("pointer-events", "auto", "important");
  });
      }

      function showNextNudge() {
        hideAllNudges();
        document.body.style.setProperty("overflow", "hidden", "important");
        document.documentElement.style.setProperty(
          "overflow",
          "hidden",
          "important"
        );
        highlightTargets.forEach((el) => {
          if (el) el.style.zIndex = "";
        });

        currentIndex++;

        if (currentIndex >= nudgeContainers.length) {
          hideAllNudges();
          localStorage.setItem(localKey, (viewCount + 1).toString());
          return;
        }

        const currentNudge = nudgeContainers[currentIndex];
        const currentHighlight = highlightTargets[currentIndex];

        if (currentNudge) currentNudge.style.display = "block";
        overlay.style.display = "block";

        // Highlight associated element
        if (currentHighlight) {
          currentHighlight.style.zIndex = "99";
          currentHighlight.style.position = "relative";
          currentHighlight.style.pointerEvents = "none";
        }
        nudgeContainers[currentIndex].style.display = "block";
        overlay.style.display = "block";
      }

      nudgeContainers.forEach((nudge) => {
        const nextBtn = nudge.querySelector(".cnt_next");
        if (nextBtn) {
          nextBtn.addEventListener("click", () => {
            showNextNudge();
          });
        }
      });

      nudgeContainers.forEach((nudge) => {
        const nextBtn = nudge.querySelector(".cnt_skip");
        if (nextBtn) {
          nextBtn.addEventListener("click", () => {
            hideAllNudges();
            currentIndex++;
            localStorage.setItem(localKey, (viewCount + 1).toString());
            return;
          });
        }
      });

      hideAllNudges();

      showNextNudge();

      updateFontSize(currentFontSize);
    });

    fontMinusBtn.addEventListener("click", () => {
      if (currentFontSize > 14) {
        gtag("event", "click", {
          event_category: "Article Tool Bar",
          event_label: "Font Size Change",
          interaction_type: "Decrease font size",
          user_action: "click",
          button_id: "btn-font-decrease",
        });

