    transparentBg.addEventListener("click", () => {
      if (!isGoldUserFlag) {
        if (promo1) {
          const isVisible = window.getComputedStyle(promo1).display === "block";
          promo1.style.display = "none";
          transparentBg.style.display = "none";
        }
        if (promo2) {
          const isVisible = window.getComputedStyle(promo2).display === "block";
          promo2.style.display = "none";
          transparentBg.style.display = "none";
        }
      }
    });

    showBtn.addEventListener("click", () => {
      if (!isGoldUserFlag) {
        if (promo1) {
          promo1.style.display = "none";
          promo2.style.display =
            window.getComputedStyle(promo2).display === "block"
              ? "none"
              : "block";
          transparentBg.style.display = "block";
