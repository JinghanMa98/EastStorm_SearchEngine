  document.addEventListener("DOMContentLoaded", function () {
    const el = document.getElementById("tracking-data");
          if (!el || typeof gtag !== "function") return;
          const isGoldUser = el.dataset.isGoldUser === "true";
          const articleTitle = el.dataset.articleTitle || "unknown_story";
          const magazineName = el.dataset.magazineName || "unknown_magazine";
          const eventLabel = `${articleTitle} - ${magazineName}`;
          const eventName = isGoldUser ? "Article Read Click" : "Article Preview Click";
          const eventData = {
            event_category: "Article Details Page",
            event_label: eventLabel,
          };
      
          gtag("event", eventName, eventData);

          // Comscore tracking for DH News articles
          const currentCategory = window.location.pathname.split('/')[2];
          
          if (currentCategory === 'dhnews') {
            // Get articleTrack data from the tracking element
            const trackingEl = document.getElementById('tracking-data');
            const articleTrackData = trackingEl?.dataset.articleTrack;
            
            if (articleTrackData) {
              try {
                const articleTrack = JSON.parse(articleTrackData);
                
                // Check if article data has pre-built Comscore URLs
                if (articleTrack && articleTrack.comscoreUrls) {
                  const container = document.getElementById('comscore-container');
                  if (container) {
                    // Use the first Comscore URL from the API and replace domain only in c9 parameter
                    let comscoreUrl = articleTrack.comscoreUrls[0];
                    comscoreUrl = comscoreUrl.replace(/c9=m\.dailyhunt\.in/g, 'c9=pwa.dailyhunt.in');
                    
                    // If c2=21733245 (BBC partnership), use window.location.href for c4
                    if (comscoreUrl.includes('c2=21733245')) {
                      comscoreUrl = comscoreUrl.replace(/c4=[^&]*/, 'c4=' + encodeURIComponent(window.location.href));
                    }
                    
                    // console.log('🎯 First Comscore URL:', comscoreUrl);
                    container.innerHTML = `<img src="${comscoreUrl}" style="display:none;" width="0" height="0" border="0" />`;
                    
                    // Use additional URLs if available
                    if (articleTrack.comscoreUrls.length > 1) {
                      setTimeout(() => {
                        let additionalUrl = articleTrack.comscoreUrls[1];
                        additionalUrl = additionalUrl.replace(/c9=m\.dailyhunt\.in/g, 'c9=pwa.dailyhunt.in');
                        
                        // If c2=21733245 (BBC partnership), use window.location.href for c4
                        if (additionalUrl.includes('c2=21733245')) {
                          additionalUrl = additionalUrl.replace(/c4=[^&]*/, 'c4=' + encodeURIComponent(window.location.href));
                        }
                        
                        // console.log('🎯 Second Comscore URL:', additionalUrl);
                        // Append second image instead of replacing
                        const secondImg = document.createElement('img');
                        secondImg.src = additionalUrl;
                        secondImg.style.display = 'none';
                        secondImg.width = 0;
                        secondImg.height = 0;
                        secondImg.border = 0;
                        container.appendChild(secondImg);
                        // console.log('✅ Second image appended to container');
                      }, 2000);
                    }
                  }
                } else {
                  // Fallback to default pixel if no Comscore URLs
                  const container = document.getElementById('comscore-container');
                  if (container) {
                    const pixelUrl = 'https://sb.scorecardresearch.com/p?c1=2&c2=21733245&c4=' + encodeURIComponent(window.location.href) + '&c9=pwa.dailyhunt.in';
                    container.innerHTML = `<img src="${pixelUrl}" style="display:none;" width="0" height="0" border="0" />`;
                  }
                }
              } catch (error) {
                // Fallback to default pixel on parsing error
                const container = document.getElementById('comscore-container');
                if (container) {
                  const pixelUrl = 'https://sb.scorecardresearch.com/p?c1=2&c2=21733245&c4=' + encodeURIComponent(window.location.href) + '&c9=pwa.dailyhunt.in';
                  container.innerHTML = `<img src="${pixelUrl}" style="display:none;" width="0" height="0" border="0" />`;
                }
              }
            }
          }
          
           

    const toolbar = document.getElementById('article-toolbar');
    const sentinel = document.getElementById('end-sentinel');

    if (!toolbar || !sentinel) return;

    const observer = new IntersectionObserver(
      ([entry]) => {
        if (entry.isIntersecting) {
          toolbar.classList.add('hidde');
        } else {
          toolbar.classList.remove('hidde');
        }
      },
      {
        root: null, 
        threshold: 0.1
      }
    );

    observer.observe(sentinel);
  });
