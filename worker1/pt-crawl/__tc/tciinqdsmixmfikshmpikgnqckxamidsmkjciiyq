    const isAvail = document.getElementById("isAvail").value;
    const issueId = document.getElementById("issueId").value;
    const articleId = document.getElementById("articleId").value;
    const selectedOne = document.getElementById("currentLang").value;
    const country = document.getElementById("country").value;
    const localLang = document.getElementById("Langcode").value;
    const isGoldUserFlag =
      document.getElementById("isGoldUserFlag").value === "true";

    const isShareActiveInput = document.getElementById("isShareActive");
    const shareIconDefault = document.getElementById("shareIconDefault");
    const shareIconActive = document.getElementById("shareIconActive");
    const isPlayingInput = document.getElementById("isPlaying");
    const speechIconPlay = document.getElementById("speechIconPlay");
    const speechIconPause = document.getElementById("speechIconPause");
    const transparentBg = document.getElementById("transparentBg");
    const shareText = document.querySelector("#showShare .cnt_spn_p");
    const speechText = document.querySelector("#showSpeech .cnt_spn_p");
    const spinner = document.getElementById("spinner");
    const fontMinusBtn = document.getElementById("fontminus");
    const fontPlusBtn = document.getElementById("dontplus");

    const langDataEl = document.getElementById("languageData");
    const localLanguages = JSON.parse(langDataEl.dataset.languages || "[]");
    const currentLangEl = document.getElementById("currentLang");
    const promo1 = document.getElementById("promo1");
