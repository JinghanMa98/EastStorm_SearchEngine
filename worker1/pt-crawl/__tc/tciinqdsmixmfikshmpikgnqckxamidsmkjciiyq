      isShareActiveInput.value = (!isActive).toString();
      shareArt.style.display = !isActive ? "block" : "none";

      shareText.style.color = !isActive ? "gold" : "#fff";
      shareIconDefault.style.display = isActive ? "block" : "none";
      shareIconActive.style.display = isActive ? "none" : "block";
    });

    searchInput.addEventListener("input", () => {
  const query = searchInput.value.trim().toLowerCase();
  const filtered = localLanguages.filter((lang) =>
    lang.display_name.toLowerCase().includes(query)
  );
  highlightedIndex = -1; 
  renderLanguages(filtered);
});

    document.addEventListener("click", (event) => {
      if (
        isShareActiveInput.value === "true" &&
        !showShare.contains(event.target) &&
        !shareArt.contains(event.target)
      ) {
        isShareActiveInput.value = "false";
        shareArt.style.display = "none";
        shareIconDefault.style.display = "block";
        shareIconActive.style.display = "none";
        shareText.style.color = "#fff";
      }
    });

    function renderLanguages(languages) {
      langItemsContainer.innerHTML = "";
highlightedIndex = -1;
      if (languages.length === 0) {
        const noResult = document.createElement("div");
        noResult.className = "language-item";
        noResult.textContent = "No languages found.";
        noResult.style.padding = "10px";
        langItemsContainer.appendChild(noResult);
        return;
      }

      languages.forEach((lang) => {
        const item = document.createElement("div");
        item.className = "language-item";
        item.style.display = "flex";
        item.style.alignItems = "center";
        item.style.justifyContent = "space-between";
        item.style.padding = "8px 12px";
        item.style.cursor = "pointer";
        item.style.borderBottom = "1px solid #eee";

        const label = document.createElement("span");
        label.textContent = lang.display_name;
        item.appendChild(label);


        if (lang.lang_code === selectedLangCode) {
          const iconWrapper = document.createElement("span");
          iconWrapper.className = "search-icon-container";
          iconWrapper.innerHTML = `
            <svg xmlns="http://www.w3.org/2000/svg" version="1.1"
                width="33" height="33" viewBox="0 0 612 792">
                <g>
                <path fill="#479f39"
                        d="M562,396c0-141.4-114.6-256-256-256S50,254.6,50,396s114.6,256,256,256S562,537.4,562,396L562,396z    
                        M501.7,296.3l-241,241l0,0l-17.2,17.2L110.3,421.3l58.8-58.8l74.5,74.5l199.4-199.4L501.7,296.3L501.7,296.3z" />
                </g>
            </svg>
            `;
          item.appendChild(iconWrapper);
        }

        item.addEventListener("click", () => {
          gtag("event", "click", {
            event_category: "Article Tool Bar",
            event_label: `Translate Article`,
            interaction_type: `Language Selected - ${lang.display_name}`,
            user_action: "click",
            button_id: `lang-${lang.lang_code}`,
          });

          selectedLangCode = lang.lang_code;
          langInput.value = lang.lang_code;
          spinner.style.display = "block";
          form.submit();
        });

        langItemsContainer.appendChild(item);
      });
    }

    const speechBtn = document.getElementById("showSpeech");
    const audioStatus = document.querySelector("#showSpeech .cnt_spn_p");
    const audioEl = document.getElementById("articleAudio");

    let isPlaying = false;
    let currentVoiceId = "1";
    let hls;
    function getAudioUrl(
      magazineId,
      issueId,
      articleId,
      language,
      country,
      voiceId
    ) {
      const baseUrl =
        location.hostname.includes("localhost") ||
        location.hostname.includes("dev.")
          ? "https://magarticles.magzter.com"
          : "https://magarticles.magzter.com";

      return `${baseUrl}/articles/${magazineId}/${issueId}/${articleId}/${language}/${country}/${voiceId}/hls/masterm3u8.m3u8`;
    }

    async function toggleAudioPlayback() {
      if (!isGoldUserFlag) {
        if (promo2) {
          promo2.style.display = "none";
          promo1.style.display =
            window.getComputedStyle(promo1).display === "block"
              ? "none"
              : "block";
          transparentBg.style.display = "block";
        }

        return;
      }

      gtag("event", "click", {
        event_category: "Article Tool Bar",
        event_label:
          isPlayingInput.value === "true" ? "Pause Audio" : "Play Audio",
        interaction_type:
          isPlayingInput.value === "true" ? "pause playback" : "start playback",
        user_action: "click",
        button_id: "btn-speech",
      });

      const url = getAudioUrl(
        magazineId,
        issueId,
        articleId,
        localLang,
        country,
        currentVoiceId
      );
      spinner.style.display = "block";

      const isPlaying = isPlayingInput.value === "true";

      if (!isPlaying) {
        if (Hls.isSupported()) {
          if (!hls) {
            hls = new Hls();
            hls.loadSource(url);
            hls.attachMedia(audioEl);
          }
          try {
            await audioEl.play();
            speechText.style.color = "gold";

            isPlayingInput.value = "true";
            audioStatus.textContent = "Pause";
            speechIconPlay.style.display = "none";
            speechIconPause.style.display = "block";
            speechText.classList.add("active-text");
            spinner.style.display = "none";
          } catch (err) {
            spinner.style.display = "none";
            console.error("Playback error:", err);
          }
        }
      } else {
        audioEl.pause();
        isPlayingInput.value = "false";
        audioStatus.textContent = "Listen";
        speechIconPlay.style.display = "block";
        speechIconPause.style.display = "none";
        speechText.classList.remove("active-text");
        speechText.style.color = "#fff";
        spinner.style.display = "none";
      }
    }

    audioEl.addEventListener("ended", () => {
      isPlayingInput.value = "false";
      audioStatus.textContent = "Listen";
      speechIconPlay.style.display = "block";
      speechIconPause.style.display = "none";
      speechText.classList.remove("active-text");
      speechText.style.color = "#fff";
      spinner.style.display = "none";
    });
 renderLanguages(localLanguages);

  
  

   function highlightItem(index) {
  console.log("Highlighting item at index:", index);
  const items = langItemsContainer.querySelectorAll(".language-item");
  items.forEach((item, i) => {
    item.style.backgroundColor = i === index ? "#eee" : "";
  });
  highlightedIndex = index;

  const highlightedEl = items[index];
  if (highlightedEl) {
    highlightedEl.scrollIntoView({ block: "nearest", behavior: "smooth" });
  }
}

document.addEventListener("keydown", (e) => {
  console.log("Key pressed:", e.key, "Lang list visible:", langList.style.display === "block");
  
  if (langList.style.right === "0px") {
    const items = langItemsContainer.querySelectorAll(".language-item");
    console.log("Items found:", items.length, "Current highlighted index:", highlightedIndex);
    
    if (!items.length) return;

    if (e.key === "ArrowDown") {
      e.preventDefault();
      highlightedIndex = (highlightedIndex + 1) % items.length;
      console.log("Arrow down - new index:", highlightedIndex);
      highlightItem(highlightedIndex);
    } else if (e.key === "ArrowUp") {
      e.preventDefault();
      highlightedIndex = (highlightedIndex - 1 + items.length) % items.length;
      console.log("Arrow up - new index:", highlightedIndex);
      highlightItem(highlightedIndex);
    } else if (e.key === "Enter") {
      e.preventDefault();
      if (highlightedIndex >= 0 && highlightedIndex < items.length) {
        console.log("Enter pressed - clicking item:", highlightedIndex);
        items[highlightedIndex].click();
      }
    }
  }
});

    showBtn.addEventListener("click", () => {
       setTimeout(() => {
    highlightedIndex = -1;
  }, 100);
    });

    speechBtn.addEventListener("click", toggleAudioPlayback);
