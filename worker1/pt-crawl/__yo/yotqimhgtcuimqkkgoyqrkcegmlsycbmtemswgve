		function replaceall( str, replacements ) {
		    if(str=='' || str==null || str==undefined) {
		    	return '';
		    }
		    var idx = str.indexOf( replacements[0] );
		    while ( idx > -1 ) {
		        str = str.replace( replacements[0], replacements[1] );
		        idx = str.indexOf( replacements[0] );
		    }
		    return str;
		}
