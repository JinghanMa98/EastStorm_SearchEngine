  var isLoaded = false;
  function asyncLoad() {
    if (isLoaded) return;
    isLoaded = true;
    var urls = ["https:\/\/unpkg.com\/@amplitude\/shopify-snippet\/snippet.js?shop=the-slate-group.myshopify.com","https:\/\/shopify-webhooks.sailthru.cloud\/shopify.js?shop=the-slate-group.myshopify.com"];
