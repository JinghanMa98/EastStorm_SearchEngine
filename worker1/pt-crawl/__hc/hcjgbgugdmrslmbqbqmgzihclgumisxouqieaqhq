                        var columnInput = document.getElementById('Lifestyle-button');

						if (!columnInput) return; // Sticky nav replaces the nav so element no longer exists for users in test.

						columnInput.addEventListener('keydown', function(e){
                            // keyCode: 13 => Enter key | keyCode: 32 => Space key
                            if (e.keyCode === 13 || e.keyCode === 32) {
                                e.preventDefault()
                                document.getElementById('Lifestyle-checkbox-input').click();
                            }
                        })
                    })</script><input type="checkbox" id="Lifestyle-checkbox-input" tabindex="-1" aria-hidden="true" class="dcr-1acea34"/><label id="Lifestyle-button" class="selectableMenuItem dcr-6mptkg" aria-label="Toggle Lifestyle" for="Lifestyle-checkbox-input" aria-haspopup="true" aria-controls="lifestyleLinks" tabindex="-1" role="menuitem" data-testid="column-collapse-Lifestyle" data-link-name="header : secondary : Lifestyle">Lifestyle</label><ul role="menu" id="lifestyleLinks" data-testid="lifestyleLinks" class="dcr-1utnfys"><li role="none" class="dcr-9ra8aa"><a class="selectableMenuItem dcr-1julzi7" href="/lifeandstyle" role="menuitem" data-link-name="header : secondary : View all Lifestyle" data-testid="column-collapse-sublink-Lifestyle" tabindex="-1">View all Lifestyle</a></li><li role="none" class="dcr-4hq641"><a class="selectableMenuItem dcr-1julzi7" href="/thefilter-us" role="menuitem" data-link-name="header : secondary : The Filter" data-testid="column-collapse-sublink-The Filter" tabindex="-1">The Filter</a></li><li role="none" class="dcr-4hq641"><a class="selectableMenuItem dcr-1julzi7" href="/us/wellness" role="menuitem" data-link-name="header : secondary : Wellness" data-testid="column-collapse-sublink-Wellness" tabindex="-1">Wellness</a></li><li role="none" class="dcr-4hq641"><a class="selectableMenuItem dcr-1julzi7" href="/fashion" role="menuitem" data-link-name="header : secondary : Fashion" data-testid="column-collapse-sublink-Fashion" tabindex="-1">Fashion</a></li><li role="none" class="dcr-4hq641"><a class="selectableMenuItem dcr-1julzi7" href="/food" role="menuitem" data-link-name="header : secondary : Food" data-testid="column-collapse-sublink-Food" tabindex="-1">Food</a></li><li role="none" class="dcr-4hq641"><a class="selectableMenuItem dcr-1julzi7" href="/tone/recipes" role="menuitem" data-link-name="header : secondary : Recipes" data-testid="column-collapse-sublink-Recipes" tabindex="-1">Recipes</a></li><li role="none" class="dcr-4hq641