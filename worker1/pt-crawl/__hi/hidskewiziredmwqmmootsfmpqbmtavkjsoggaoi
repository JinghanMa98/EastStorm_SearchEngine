  <script>
    try {
      const mastheadChildren = document.currentScript.previousElementSibling.children;
      const posFromBack = document.cookie.indexOf("__utp") === -1 ? 2 : 1;
      mastheadChildren[mastheadChildren.length - posFromBack].hidden = false;
    } catch(err) {
      console.error("Error unhiding sign-in/account in masthead", err);
    }
  </script>
