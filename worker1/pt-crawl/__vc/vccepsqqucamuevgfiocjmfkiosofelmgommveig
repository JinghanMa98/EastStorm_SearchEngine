    var loggedIn = 0;                                           // track authentication
    $.getScript("https://web.archive.org/web/20130316055457/http://connect.facebook.net/en_US/all.js");
    window.fbAsyncInit = function(callback, anonParamObj) {
        FB.init({
            appId: '146078808769941',                                 // App ID
            channelUrl: 'https://web.archive.org/web/20130316055457/http://www.postandcourier.com/section/channel',    // Channel File
            status: true,                                       // check login status
            cookie: true,                                       // enable cookies to allow the server to access the session
            xfbml: true                                         // parse XFBML
        });
		if($.isFunction(callback)){
			FB.getLoginStatus(function(response) {
			    if (response.status === 'connected') {
			        callback(anonParamObj,response.status);
			    } else if (response.status === 'not_authorized') {
			        callback(anonParamObj,response.status);
			    } else {
				    callback(anonParamObj,response.status);  
			    }
			});
        }
    }  // end fbAsyncInit
    /* Load the SDK Asynchronously
    (function(d){
        var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
        if (d.getElementById(id)) {return;}
        js = d.createElement('script'); js.id = id; js.async = true;
        js.src = "https://web.archive.org/web/20130316055457/http://connect.facebook.net/en_US/all.js";
        ref.parentNode.insertBefore(js, ref);
    }(document));
    */
</script>
