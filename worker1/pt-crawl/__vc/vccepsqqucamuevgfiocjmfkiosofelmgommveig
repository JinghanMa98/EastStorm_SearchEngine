<div id="commentcontainer"></div>
<script type="text/javascript">
    var commentsPerPage = 5                     //this is the default, it can be overridden by a value from the macro
    var totalUserComments = 0;                  //default total number of comments is 0
    var userCommentsStart = 0;                  //start the "page" on the 0th comment
    var userStatus = 4;                         //default to not logged in
    var commentsXML = null;
    // Get Comment Per Page
    if('' != ''){
        commentsPerPage = parseInt('');   
    }
    var userCommentsEnd = commentsPerPage; //end the first "page" at the end of the first slice
    $.reportAbusePopup = function(mypage,myname,w,h,scroll,pos){
        var win=null;
        if(pos=="random"){
            LeftPosition=(screen.width)?Math.floor(Math.random()*(screen.width-w)):100;
            TopPosition=(screen.height)?Math.floor(Math.random()*((screen.height-h)-75)):100;
        }
        if(pos=="center"){
            LeftPosition=(screen.width)?(screen.width-w)/2:100;
            TopPosition=(screen.height)?(screen.height-h)/2:100;
        }else if((pos!="center" && pos!="random") || pos==null){
            LeftPosition=0;
            TopPosition=20;
        }
        settings='width='+w+',height='+h+',top='+TopPosition+',left='+LeftPosition+',scrollbars='+scroll+',location=no,directories=no,status=no,menubar=no,toolbar=no,resizable=yes';
        win=window.open(mypage,myname,settings);
    };
    $.toggleUserCommentNavigation = function(){ 	
    if((userCommentsStart + commentsPerPage) < totalUserComments){
        $('#usercommentsnexttop,#usercommentsnextbottom').css('display','inline');
    }
    else{
        $('#usercommentsnexttop,#usercommentsnextbottom').css('display','none');
    }
    if(userCommentsStart > 1){
        $('#usercommentsprevioustop,#usercommentspreviousbottom').css('display','inline');
    }
    else{
        $('#usercommentsprevioustop,#usercommentspreviousbottom').css('display','none');
    }
    var page = Math.ceil(userCommentsStart/commentsPerPage);
    var totalpage = Math.floor(totalUserComments/commentsPerPage);
    $('#page').html(page+1);
    $('#totalpage').html(totalpage+1);
    if(totalUserComments >= 5){
        $('#pagetracker').css('display','block');	
    }
};
$.toggleUI = function(userStatus,postlink,commentsLocked){
    if(commentsLocked == 0){
        if(userStatus == 'loggedIn'){
            $('#submitcontainer').css('display','block');
            $('#newmessageform').attr('action',postlink);
            $('#RedirectURL').attr('value',window.location);
            $('#signincontainer').css('display','none');
        } else { 
            $('#submitcontainer').css('display','none');
            $('#signincontainer').css('display','block');
        }   
    } else {
        $('#submitcontainer').css('display','none');
        $('#signincontainer').css('display','none');
    }
};
$.setCommentsTotal = function(totalUserComments){ 
    $('#total-comments').html(totalUserComments);	
    $('#userCommentsTopBar').css('display','block');
};
$.parseXml = function(xml){ 
	/*
	Begin code from Jeremy Hixon - http://jeremyhixon.com/jquery-ajax-internet-explorer/#.T6lIHsWD83F
	"Internet Explorer wont parse XML quite as easily as every other 
	browser on the internet (I assume). You have to change what you pass 
	it as through jQuery. In IE it has to be passed as text, while the 
	rest can handle it as XML. You also have to pass it through a separate 
	function that brings in ActiveX before you can navigate your way 
	through the XML and use it in your application."
	*/
      if (jQuery.browser.msie) {  
        var xmlDoc = new ActiveXObject("Microsoft.XMLDOM");  
        xmlDoc.loadXML(xml);  
        xml = xmlDoc;  
      }  
      return xml;     
};
$.loadInComments = function(callBack){
	$('#comments').block({ 
        message: '<div class="comment-loading">Loading...</div>'
    }); 
    //load in gen_loadComments.pbs XML
    $.ajax({
        url: "https://web.archive.org/web/20130316055457/http://www.postandcourier.com/article/20110317/PC20/&template=loadComments&v="+Math.random(),
        cache: false,
        dataType: ($.browser.msie) ? "text" : "xml",
        success: function(data){
            $('#comments').unblock();
            data = $.parseXml(data);
            var commentsEnabled = $(data).find('commentsenabled').text();
            if(commentsEnabled == 1){ //if comments are enabled proceed, otherwise this script essentially ends
                totalUserComments = $(data).find('totalusercomments').text();
                userStatus = $(data).find('userstatus').text();
                postlink = unescape($(data).find('postlink').text());
                //set Comments total in User Comments top bar
                $.setCommentsTotal(totalUserComments);
                var commentsLocked = $(data).find('commentslocked').text();
                //make the correct part of the UI visible
                $.toggleUI(userStatus,postlink,commentsLocked);
                //parse out and store comments XML in commentsXML variable
                commentsXML = $(data).find('usercomments').children();
                //adjust navigation controls
                $.toggleUserCommentNavigation();
                //run the call back which is basically the changeOutComments function
                callBack();   
            }
        } //end anonymous function on ajax success
    }); //end ajax 
};
$('#usercommentsnexttop,#usercommentsnextbottom').click(function(event){
    event.preventDefault();
    userCommentsStart = userCommentsStart + commentsPerPage;
    userCommentsEnd = userCommentsEnd + commentsPerPage;
    $.toggleUserCommentNavigation()
    $.changeOutComments();
});
$('#usercommentsprevioustop,#usercommentspreviousbottom').click(function(event){
    event.preventDefault();
    userCommentsStart = userCommentsStart - commentsPerPage;
    userCommentsEnd = userCommentsEnd - commentsPerPage;
    $.toggleUserCommentNavigation()
    $.changeOutComments();
});
$.changeOutComments = function(){ 
    //empty out any comments currently displaying
    $('#commentcontainer').html('');
    $(commentsXML).slice(userCommentsStart,userCommentsEnd).each(function(){
       var template = '<div class="comment clearfix">';
       template = template + '<h3><span>'+unescape($(this).find('postedbyfirstname').text())+' '+unescape($(this).find('postedbylastname').text())+'</span></h3>';
       template = template + '<p class="meta">'+unescape($(this).find('date').text())+'</p>';
       template = template + '<div class="comment-vote">';
       template = template + '<ul>';
       template = template + '<li class="vote-up-down"><span class="vote-down"><img src="/images/1336859884_001_19.gif" onclick="$.vote(\''+$(this).find('novotelink').text()+'\',this);"></span><span class="vote-up"><img src="/images/1336858909_001_18.gif" onclick="$.vote(\''+$(this).find('yesvotelink').text()+'\',this);"></span></li>';       
       template = template + '<li class="vote-result"><span class="down-result">'+unescape($(this).find('novotecount').text())+'</span><span class="up-result">'+unescape($(this).find('yesvotecount').text())+'</span></li>';
       template = template + '</ul>';
       template = template + '</div>';       
       template = template + '<div class="comment-body">';
       template = template + '<p>'+unescape($(this).find('commentbody').text())+'</p>';
       template = template + '</div>';
       template = template + '<div class="comment-report"><a onclick="$.reportAbusePopup(\'/apps/pbcs.dll/section?template=reportAbuse&amp;author='+$(this).find('postedbyfirstname').text()+'+'+$(this).find('postedbylastname').text()+'&amp;createdDate='+$(this).find('date').text()+'&amp;body='+$(this).find('commentbody').text()+'&amp;storyLink=http%3A%2F%2Fcpdev.sx2.atl.publicus.com%2Fapps%2Fpbcs.dll%2Farticle%3FAID%3D%2F20110317%2FPC20%2F303179904&nocache=1\',\'Report Abuse\',400,400,0,\'center\'); return false;" href="">Report Abuse</a></div>';
       template = template + '<div class="unauthorize" style="margin: 10px 0; color: red; font-weight: bold;"></div>';
       template = template + '</div>';           
       $('#commentcontainer').append(template);    
	});	
};
$(document).ready(function(){
	$.loadInComments(function(){
    	$.changeOutComments();
	});
}); //end ready
$.checkUserStatusViaFacebookAndConditionallySubmitVote = function(anonParamObj, userStatus){
	console.log(userStatus, status);
	if (userStatus === 'connected') {
        $("#comments").block({ 
            message: '<div class="comment-loading">Processing vote...</div>'
        });
		$.ajax({
			url: unescape(anonParamObj.voteLink)+"&v="+Math.random(),
			cache: false,
			success: function(data){
				$("#comments").unblock(); 
				if(data == 0){
					$(".unauthorize").html('You may vote on each comment once');
				} else {
					$.loadInComments(function(){$.changeOutComments();});
				}
			}
		});
  } else if (status === 'not_authorized') {
    $(".unauthorize").html('You must login to vote!');
  } else {
    $(".unauthorize").html('You must login to vote');
  }
}; 
$.vote = function(voteLink, obj){
    var anonParamObj=new Object();
    anonParamObj.voteLink=voteLink;
    anonParamObj.obj=obj;	
    fbAsyncInit($.checkUserStatusViaFacebookAndConditionallySubmitVote, anonParamObj);
};
</script>		
</section>
