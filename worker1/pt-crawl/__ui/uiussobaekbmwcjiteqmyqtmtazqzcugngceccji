    <script>
        function toggleNotification(elId) {
            ignoreHideMF = true;
            var element = document.getElementById(elId);
            element.style.display = element.style.display === 'none' ? 'block' : 'none';
        }

        function closeNotification(elId) {
            var element = document.getElementById(elId);
            element.style.display = 'none';
        }

        var ignoreHideMF = false;
        window.onclick = function() {
            if (!ignoreHideMF) {
                var tooltips = document.getElementsByClassName('mf-tooltip');
                if (tooltips) {
                    for (var i = 0; i < tooltips.length; i++) {
                        if (tooltips[i].style.display === 'block')
                            tooltips[i].style.display = 'none';
                    }
                }
            }
            ignoreHideMF = false;
        };

        document.addEventListener("DOMContentLoaded", function(event) {
            var mfToolTips = document.querySelectorAll('.mf-tooltip');
            if (mfToolTips) {
                for (var i = 0; i < mfToolTips.length; i++) {
                    mfToolTips[i].onclick = function(e) {
                        e.stopPropagation();
                    }
                }
            }
        });


    </script>


                    


