Mntl.digioh.types.load();
}
// Custom event needed to prevent circular dependencies on the timers
window.dispatchEvent(new CustomEvent('digiohLoaded'));
}
let inactivityTimer = setTimeout(loadDigioh, Mntl.digioh.timerDelay);
