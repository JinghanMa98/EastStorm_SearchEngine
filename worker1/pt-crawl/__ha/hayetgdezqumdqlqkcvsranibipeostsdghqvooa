<script type="text/javascript">
Mntl.utilities.scriptsOnLoad(document.querySelectorAll('script[data-glb-js="bottom"]'), function() {
var Mntl = window.Mntl || {};
window.Mntl = window.Mntl || {};
window.Mntl.affiliatePlugins = window.Mntl.affiliatePlugins || [];
window.Mntl.affiliateLinkRewriter.setMappings( {
DOC_ID: '8686909'
,SITE: 'people'
,REQUEST_ID: 'n8faf4beedf474701993d7896d6b8f50a20'
}
);
window.Mntl.affiliatePlugins.push(window.Mntl.affiliateLinkRewriter.processLink);window.Mntl.externalizeLinks.addPlugin(Mntl.externalizeLinkDefaultPlugin);
window.Mntl.externalizeLinks.init();(function (utils, trackingUtils) {
const peopleComments = document.querySelector('.mntl-viafoura-comments');
const bellTrigger = document.querySelector('.people-vf-trigger');
const TOKEN_DURATION = 1000 * 60 * 5; // 5 Minutes
const CHECK_REFRESH_INTERVAL = 1000 * 60 * .25; // 15 Seconds
bellTrigger.addEventListener('click', () => {
const trackingEvent = trackingUtils.createEventObject();
/* eslint-disable camelcase -- disabling camelcase requirement to allow event data variables as agreed upon here https://dotdash.atlassian.net/l/cp/hXCUMfuT */
trackingEvent.event_params.event_type = 'Click';
trackingEvent.event_params.event_category = 'Viafoura Comments';
trackingEvent.event_params.event_metadata = (false) ? true : null;
trackingEvent.event_params.event_subcategory = 'Notification Tray';
trackingEvent.event_params.event_subtype = 'Nav Open';
trackingEvent.event_params.event_target_url = null;
/* eslint-enable camelcase */
utils.callAsync(() => utils.pushToDataLayer(trackingUtils.refiningDdmEventData(trackingEvent))());
}, true);
// KEYCLOAK AUTHENTICATION ROUTERS
function loginDDM() {
const signIn = bellTrigger.getAttribute('data-sign-in');
redirectToAuth(signIn);
}
function signUpDDM() {
const signUp = bellTrigger.getAttribute('data-sign-up');
redirectToAuth(signUp);
}
// REFRESH INTERVAL
function initRefreshInterval() {
return setInterval(() => {
const isLoggedIn = utils.isLoggedIn();
const isPageVisible = document.visibilityState === 'visible';
const shouldRefresh = Date.now() > localStorage.getItem("ddmTokenExpiry");
if (isLoggedIn && isPageVisible && shouldRefresh) {
utils.refreshIdToken()
.then(() => {
loginViafoura();
})
.catch(() => {
logoutViafoura();
});
}
}, CHECK_REFRESH_INTERVAL);
}
function onTrayLoaded() {
if (utils.isLoggedIn()) {
utils.refreshIdToken()
.then(() => {
loginViafoura();
interval = initRefreshInterval();
})
.catch(() => {
logoutViafoura();
});
} else {
logoutViafoura();
}
}
function onPrepublish(channel, event, ...args) {
if (channel === 'authentication' && event === 'required') {
window.vf.$publish("tray", "close");
const { method } = args[0];
onAuthenticate(method);
return false;
}
return {
channel,
event,
args
};
}
// VIAFOURA EVENT HANDLERS
function onAuthenticate(method){
if (utils.isLoggedIn()) {
utils.refreshIdToken()
.then(() => {
loginViafoura();
})
.catch(() => {
if (method === "login"){
loginDDM();
} else {
signUpDDM();
}
});
} else if (method === "login"){
loginDDM();
} else {
signUpDDM();
}
}
// HELPER FUNCTIONS
function redirectToAuth(authUrl) {
window.location = authUrl;
}
// VIAFOURA AUTHENTICATION METHODS
function loginViafoura() {
try {
const { idToken } = utils.isLoggedIn({ shouldReturnDdmAccountObject: true });
window.vf.session.login.openIdConnect(idToken);
localStorage.setItem("ddmTokenExpiry", Date.now() + TOKEN_DURATION);
} catch (error) {
console.error('Error logging into Viafoura', error);
}
}
function logoutViafoura() {
try {
window.vf.session.logout();
clearInterval(interval);
localStorage.removeItem("ddmTokenExpiry");
} catch (error) {
console.error('Error logging out of Viafoura', error);
}
}
if (peopleComments === null) {
window.Mntl = window.Mntl || {};
Mntl.Viafoura = (function(utils) {
const init = () => {
// Load Viafoura Lib
utils.loadExternalJS({
src: '//cdn.viafoura.net/entry/index.js',
class: 'viafoura-lib',
async: 'true'
});
};
window.addEventListener('readyForThirdPartyTracking', init);
})(window.Mntl.utilities || {});
window.vfQ = window.vfQ || [];
window.vfQ.push(() => {
window.vf.$prepublish(onPrepublish);
window.vf.$subscribe('vf-tray-trigger', 'loaded', onTrayLoaded);
});
}
})(window.Mntl.utilities || {}, window.Mntl.trackingUtilities || {});
(function(Mntl) {
Mntl.Tooltip.init({
defaultPositionX: "auto",
defaultPositionY: "auto"
});
})(window.Mntl || {});
