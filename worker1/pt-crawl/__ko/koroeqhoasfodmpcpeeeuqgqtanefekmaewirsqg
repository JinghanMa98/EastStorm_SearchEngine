<script>(function() {
  var isLoaded = false;
  function asyncLoad() {
    if (isLoaded) return;
    isLoaded = true;
    var urls = ["https:\/\/cdn.xopify.com\/xo-scroll2top\/xo-scroll2top2.js?shop=rouleur-emporium.myshopify.com","https:\/\/na.shgcdn3.com\/pixel-collector.js?shop=rouleur-emporium.myshopify.com",""];
