    <script type="text/javascript" class='optanon-category-C0001'>
        function hoverTooltip(articleId) {
            document.querySelectorAll(`#article-${articleId} .reviewer-tooltip`).forEach( node => {
                node.addEventListener("mouseover", function() {
                    let self = this;
                    this.querySelector(".tooltiptext").style.display = "block";
                    setTimeout(() => {
                        self.setAttribute('onclick',"toggleTooltip(this);")
                    })
                });
                node.addEventListener("mouseout", function() {
                    this.querySelector(".tooltiptext").style.display = "none";
                });
            });
        }

        function toggleTooltip(self) {
            self.querySelector(".tooltiptext").style.display =  self.querySelector(".tooltiptext").style.display == "none" ? "block" : "none";
        }
    </script>

