    //Delay
    function divCheck(itr) {
        if(itr < 200) {
            for(var key in adUnits) {
                if(!document.getElementById(adUnits[key]['pos'])) {
                    setTimeout(function() {
                        divCheck(itr+1)
                    }, 200);
                    return false;
                }
            }
            defineAdSlots(adUnits, true);
        }

    }
    initDfp();

