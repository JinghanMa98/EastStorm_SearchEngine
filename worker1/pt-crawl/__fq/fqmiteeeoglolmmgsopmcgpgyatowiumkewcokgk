    for (let key in options) dfpOptions[key] = options[key];
    //console.log(dfpOptions)

    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];


    // Start apstag
    if (vendorsApstag) {
        var apstagSlotLevel = 'page';

        //amazon a9
        if (!window["apstag"]) {
            window["apstag"] = {
                init: function () {
                    window["apstag"]._Q.push(["i", arguments])
                },
