        document.addEventListener('DOMContentLoaded', function () {
            document.querySelectorAll(".child-menu > li").forEach(node => {
                node.addEventListener('mouseover', function (e) {
                    var menuItemRect = this.parentNode.parentNode.getBoundingClientRect();

                    if (768 < window.innerWidth && window.innerWidth < 1000 && this.parentNode.previousSibling.text.includes("Forums")) {
                        this.parentNode.style.left = "0px";
                        this.querySelectorAll('ul').forEach(ul => {
                            ul.style.top = document.querySelector('nav.diy-navbar').offsetHeight + "px";
                            ul.style.left = (menuItemRect.x + this.parentNode.offsetWidth - 140) + "px";
                            ul.style.minHeight = this.parentNode.offsetHeight + "px";
                        });
                    } else {
                        this.querySelectorAll('ul').forEach(ul => {
