<script type="text/javascript">
    function OptanonWrapper() { }
    function hasClass(el, cls) {
        cls = cls || '';
        if(cls.replace(/\s/g, '').length == 0) return false;
        return new RegExp(' ' + cls + ' ').test(' ' + el.className + ' ');
    }

    function hasParentNode(node) {
        return node && typeof node.hasOwnProperty === 'function' && node.hasOwnProperty('parentNode');
    }

    window.waitForJQuery = function() {
        return new Promise((resolve) => {
            const checkjQuery = () => {
            if (window.jQuery) {
                resolve();
            } else {
                setTimeout(checkjQuery, 100); // Check again after 100 milliseconds
            }
            };
            checkjQuery(); // Start checking immediately
        });
    }

    window.waitForGalleria = function() {
        return new Promise((resolve) => {
            const checkGalleria = () => {
            if (window.jQuery && (window.runGalleria || window.Galleria)) {
                resolve();
            } else {
                setTimeout(checkGalleria, 100); // Check again after 100 milliseconds
            }
            };
            checkGalleria(); // Start checking immediately
        });
    }
</script>
