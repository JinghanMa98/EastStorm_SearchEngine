    <script type="text/plain" class='optanon-category-C0004'>
        var PWT={}; //Initialize Namespace
        var gptRan = false;
        PWT.jsLoaded = function(){ //PubMatic pwt.js on load callback is used to load GPT
            loadGPT();
        };
        var loadGPT = function() {
            // Check the gptRan flag
            if (!gptRan) {
            gptRan = true;
            var gads = document.createElement('script');
            var useSSL = 'https:' == document.location.protocol;
            gads.src = (useSSL ? 'https:' : 'http:') + '//securepubads.g.doubleclick.net/tag/js/gpt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(gads, node);
            }
        };
        // Failsafe to call gpt. 500ms timeout can be updated as per publisher preference.
        setTimeout(loadGPT, 500);
        
        
        (function() {
            var purl = window.location.href;
            var url = '//ads.pubmatic.com/AdServer/js/pwt/163302/13199';
            var profileVersionId = '';
            if(purl.indexOf('pwtv=')>0){
                var regexp = /pwtv=(.*?)(&|$)/g;
                var matches = regexp.exec(purl);
                if(matches.length >= 2 && matches[1].length > 0){
                    profileVersionId = '/'+matches[1];
                }
            }
            var wtads = document.createElement('script');
            wtads.async = true;
            wtads.type = 'text/javascript';
            wtads.src = url+profileVersionId+'/pwt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(wtads, node);
        })();
    </script>


<script type="text/plain" class='optanon-category-C0004'>
    window.v = 2;
    const DATA_PERCENT_VIEW = "percent-viewport";
    const vendorsApstag = true;
    //adSlotsMap = {adId: [object, last_refreshed_time initialized with -1]}, Map<string,Array<object,int>>
    var adSlotsMap = new Map();
    window.isSponsoredArticle = false;

    function getVisibility(elem, setAttr = true) {
        var visible = 0;
        var elemRect = elem.getBoundingClientRect();
        var elemTop = elemRect.top;
        var elemBottom = elemRect.bottom;

        if (elemBottom < 0 || elemTop > window.innerHeight) {
            // If the element is completely outside of viewport
            visible = 0;
        } else if ((elemTop <= 0 && elemBottom >= window.innerHeight) || (elemTop > 0 && elemBottom < window.innerHeight)) {
            // If the element is completely within of viewport
            // or if the top of element is above viewport and bottom of element is below viewport
            visible = 100;
        } else if (elemTop > 0) {
            // if top is within viewport
            visible = ((window.innerHeight - elemTop) / window.innerHeight) * 100;
        } else {
            // assuming bottom is within viewport
            visible = (elemBottom / window.innerHeight) * 100;
        }

        if (setAttr) {
            elem.setAttribute('data-percent-view', visible);
        }

        return visible;
    }


    function refreshAds(adsSlotsToRefresh) {
        /*
        * Function accepts array of ads slots ID [Can be obtained by obj.getSlotId()]
        *   Ad slot can't be refreshed within 30 seconds time frame
        */
        let adsToRefresh = []; // Holds the ads slots that will be refreshed
        const minimumViewTime = 30; // Time in seconds
        let currentTime = new Date().getTime()

        for (const i in adsSlotsToRefresh) {
            if (adSlotsMap.get(adsSlotsToRefresh[i]) === undefined) continue;
            let adViewTime = (currentTime - adSlotsMap.get(adsSlotsToRefresh[i])[1]) / 1000

            //Checks if ad never been refreshed or it have been displayed longer then the minimumViewTime
            let canRefreshAd = adSlotsMap.get(adsSlotsToRefresh[i])[1] === -1 || adViewTime >= minimumViewTime;
            if (canRefreshAd && adSlotsMap.get(adsSlotsToRefresh[i])[0] !== "undefined" && typeof adSlotsMap.get(adsSlotsToRefresh[i])[0].getSlotId !== "undefined") {
                adsToRefresh.push(adSlotsMap.get(adsSlotsToRefresh[i])[0])
                adSlotsMap.get(adsSlotsToRefresh[i])[1] = currentTime;
            }
        }

        if (adsToRefresh.length > 0) {
            googletag.pubads().refresh(adsToRefresh);
        }
    }


    function removeAdFromMap(adIds) {
        /*
        *
        * Function that accepts ads ids and remove them from the AdSlotsMap after being destroyed (out of view)
        * This function is being called from the ibInjectAd.vue
        *
        * */
        adIds.forEach(e => {
            if(e && typeof e.getSlotId !== "undefined")
                adSlotsMap.delete(String(e.getSlotId()))
        });
    }


    function defineAdSlots(adUnitCollection, processApsTags) {
        googletag.cmd.push(function () {
            for (let key in adUnitCollection) {
                let _slot = adUnitCollection[key];
                let _path = _slot.path;
                let _pos = _slot.pos;
                let _s = '';

                if ("oop" in _slot) {
                    _s = googletag.defineOutOfPageSlot(_path, _pos)
                } else {
                    _s = googletag.defineSlot(_path, _slot.sizes, _pos);
                }

                if ("target" in _slot) {
                    const _target = _slot.target;
                    for (let index in _target) {
                        let targetKey = index.toLowerCase();
                        _s.setTargeting(targetKey, _target[index]);
                    }
                }

                try {
                    _s.addService(googletag.pubads())
                } catch (err) {
                    // console.log('unabled to render ad', _pos)
                    // console.log('err', err)
                    continue;
                }

                window.adSlots[key] = _s; //for dfpAd component
                gptadslots.push(_s);
                adSlotsMap.set(String(_s.getSlotId()), [_s, -1]);

                //Moved from IbInjectAd.vue
                googletag.display(_pos)
            }

            if (vendorsApstag && processApsTags) {
                processApstag(gptadslots);
            }
        })
    }

    /**
     * Pass an array of keys and will go through window.adSlots to see which keys are in window.adSlots
     * @param Array<String> keys
     * @return Array<GPT Object>
     */
    function getSlots(keys) {
        let list = [];
        for(let i = 0; i < keys.length; i++) {
            let key = keys[i];
            if(key in window.adSlots) {
                list.push(window.adSlots[key]);
            }
        }

        return list;
    }

    /**
     * Pass an array of keys and will go through adUnits to see which keys are in adUnits
     * @param Array<String> keys
     * @return Object
     */
    function getAdUnits(keys) {
        let list = {};
        for(let i = 0; i < keys.length; i++) {
            let key = keys[i];
            if(key in adUnits) {
                list[key] = adUnits[key];
            }
        }

        return list;
    }


    function initDfp() {
        googletag.cmd.push(function () {

            const adsService = googletag.pubads()

            if (dfpOptions.setForceSafeFrame) {
                adsService.setForceSafeFrame(true)
            }

            if (dfpOptions.enableAsyncRendering) {
                adsService.enableAsyncRendering()
            }

            if (dfpOptions.enableSingleRequest) {
                adsService.enableSingleRequest()
            }


            for (let x in dfpOptions.setTargeting) {
                adsService.setTargeting(x, dfpOptions.setTargeting[x])
            }

            const setLocation = dfpOptions.setLocation

            if (typeof setLocation === 'object') {
                if (typeof setLocation.latitude === 'number' && typeof setLocation.longitude === 'number' &&
                    typeof setLocation.precision === 'number') {
                    adsService.setLocation(setLocation.latitude, setLocation.longitude, setLocation.precision)
                } else if (typeof setLocation.latitude === 'number' && typeof setLocation.longitude === 'number') {
                    adsService.setLocation(setLocation.latitude, setLocation.longitude)
                }
            }

            if (dfpOptions.setCategoryExclusion.length > 0) {
                const exclusionsGroup = dfpOptions.setCategoryExclusion.split(',')
                let valueTrimmed;
                exclusionsGroup.forEach(function (v) {
                    valueTrimmed = v.trim()
                    if (valueTrimmed.length > 0) {
                        adsService.setCategoryExclusion(valueTrimmed)
                    }
                })
            }

            if (dfpOptions.collapseEmptyDivs && !dfpOptions.enableLazyLoad) {
                adsService.collapseEmptyDivs()
            }

            if (dfpOptions.enableLazyLoad) {
                if (typeof (dfpOptions.enableLazyLoad) == "object") {
                    adsService.enableLazyLoad(dfpOptions.enableLazyLoad)
                } else {
                    adsService.enableLazyLoad()
                }
            }

            if (dfpOptions.disablePublisherConsole) {
                adsService.disablePublisherConsole()
            }

            if (dfpOptions.companionAds) {
                googletag.companionAds().setRefreshUnfilledSlots(true)
                if (!dfpOptions.disableInitialLoad) {
                    adsService.enableVideoAds()
                }
            }

            if (dfpOptions.disableInitialLoad) {
                adsService.disableInitialLoad()
            }

            if (dfpOptions.setCentering) {
                adsService.setCentering(true)
            }

            if (dfpOptions['afterEachAdLoaded']) {
                googletag.pubads().addEventListener('slotRenderEnded', dfpOptions['afterEachAdLoaded'])
            }

            // If the user declines "C0004", and belongs to the us compliant states: 'ca', 'va', 'co', 'ct'
            // Set the nonPersonalizeAds to true
            if (window.OnetrustActiveGroups && !window.OnetrustActiveGroups.includes('C0004')) {
                if(window.OneTrust && typeof window.OneTrust.getGeolocationData === 'function') {
                   let area = window.OneTrust.getGeolocationData();
                   if(area.country.toLowerCase() == 'us' && ['ca', 'va', 'co', 'ct'].indexOf(area.state.toLowerCase()) >= 0) {
                      googletag.pubads().setPrivacySettings({nonPersonalizedAds: true});
                   }
                }
            }

            googletag.enableServices();

            // Max Time to wait For sticky Ad to Render
            setTimeout(function () {
                let temp_sticky_ad_var = document.querySelectorAll('.temp-sticky-ad');
                if (temp_sticky_ad_var) {
                    temp_sticky_ad_var.forEach(function (e) {
                    unstickAd(e);
                    })
                }
            }, 5000);

            googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                arrAdsTimesTable[event.slot.getSlotElementId()] = new Date().getTime() / 1000;
            });

            googletag.pubads().addEventListener('slotOnload', function (event) {
                let slotDiv = document.querySelector('#'+event.slot.getSlotElementId())
                if (hasClass(slotDiv,"temp-sticky-ad")) {
                    unstickAd(slotDiv);
                } else if (hasClass(slotDiv.parentElement,'temp-sticky-ad').length) {
                    unstickAd(slotDiv.parentElement);
                }
                stabilizeInjectedAdHeight(slotDiv)
            });

            googletag.pubads().addEventListener('slotVisibilityChanged', function (event) {
                document.getElementById(event.slot.getSlotElementId()).setAttribute("data-" + DATA_PERCENT_VIEW,event.inViewPercentage)
            });
        })

    }

    function stabilizeInjectedAdHeight(slotDiv) {
        if (hasClass(slotDiv.parentElement,'ibinject-container')) {
            let minHeight = parseInt((slotDiv.style.minHeight).replace(/px$/, ''));
            if (minHeight < slotDiv.style.height) {
                slotDiv.style.minHeight=slotDiv.style.height;
            }
        }
    }

    function unstickAd(element) {
        if (!isNaN(maxStickyTime = parseInt(element.getAttribute("data-temp-sticky-ad-time")))) {
            window.scroll(function () {
                setTimeout(function () {
                    element.classList.remove('temp-sticky-ad');
                }, maxStickyTime);
            });
        } else {
            if (dfpOptions.setTargeting.environment !== 'production') {
                console.error("Unable to unstick element for sticky Ad:", element);
            }
        }
    }

    function processApstag(arrSlots) {
        if (typeof apstagSlotLevel === 'undefined' || typeof window['apstag'] === 'undefined')
            return false;

        var apstagSlots = [], arrGptAdRefrash = [], arrApstagSlots = [];
        for (let j = 0; j < arrSlots.length; j++) {
            let oSlot = arrSlots[j], arrSizes = [], tagId = oSlot.getSlotElementId(), slotName = oSlot.getAdUnitPath(),
                oSlotSizes = oSlot.getSizes();
            for (let i in oSlotSizes) {
                arrSizes.push([oSlotSizes[i].getWidth(), oSlotSizes[i].getHeight()]);
            }

            if (apstagSlotLevel == 'page') {
                apstagSlots.push({slotID: tagId, slotName: slotName, sizes: arrSizes});
                arrApstagSlots.push(oSlot);
            } else if (apstagSlotLevel == 'slot') {
                let device = 'desktop', idPattern = 'div-gpt-ad-743021889055430906';

                var bAddToApstag = arrAdslots.filter(function(element) {
                    var elementTagId = idPattern + '-' + element.id + '-' + device;
                    if (elementTagId === tagId && element.thirdparties !== null) {
                        var arrApstag = element.thirdparties.filter(function(e) {
                            return e.name === "apstag" && e.enabled === true;
                        });
                        return arrApstag.length > 0;
                    }
                    return false;
                })

                if (bAddToApstag.length) {
                    apstagSlots.push({slotID: tagId, slotName: slotName, sizes: arrSizes});
                    arrApstagSlots.push(oSlot);
                } else {
                    arrGptAdRefrash.push(oSlot);
                }
            }
        }

        if (apstagSlots.length > 0 && apstagSlots.length === arrApstagSlots.length) {
            apstag.fetchBids({
                slots: apstagSlots
            }, function () {
                googletag.cmd.push(function () {
                    apstag.setDisplayBids();
                    refreshAds(arrApstagSlots.map(a => String(a.getSlotId())));
                });
            });
        }

        if (arrGptAdRefrash.length > 0) {
            refreshAds(arrGptAdRefrash.map(a => String(a.getSlotId())));
        }
        return true;
    }

    function processAdSlots(arrSlots) {
        let bProcessApstag = processApstag(arrSlots);
        if (!bProcessApstag) {
            refreshAds(arrSlots.map(a => String(a.getSlotId())));
        }
    }

    function checkForSponsoredArticle(element) {
        let unitIds = ["300x250_top","160x600_right"];
        if(element && typeof element === "object") {
            let rightrailAds = [];

            function buildInit(isShowing) {
                rightrailAds = getSlots(unitIds);

                if(rightrailAds.length == 0 && isShowing) {
                    let rightrailUnits = getAdUnits(unitIds);
                    defineAdSlots(rightrailUnits,false);
                    googletag.cmd.push(function() {
                        processAdSlots(getSlots(unitIds))
                    });
                }
            }

            if(element.getAttribute('data-sponsored') && !window.isSponsoredArticle) {
                window.isSponsoredArticle = true;
                buildInit(false);

                //Hide right rail ads
                if(rightrailAds.length > 0)  {
                    googletag.cmd.push(function() {
                        removeAdFromMap(rightrailAds);
                        googletag.pubads().clear(rightrailAds);
                    });
                }
            } else if(!element.getAttribute('data-sponsored') && window.isSponsoredArticle) {
                window.isSponsoredArticle = false;
                buildInit(true);

                //Show right rail ads
                if(rightrailAds.length > 0) {
                    for(var i = 0; i < rightrailAds.length; i++) {
                        adSlotsMap.set(String(rightrailAds[i].getSlotId()), [rightrailAds[i], -1]);
                    }
                    processAdSlots(rightrailAds);
                }
            }
        }
    }

    function hasClass(el, cls) {
        cls = cls || '';
        if(cls.replace(/\s/g, '').length == 0) return false;
        return new RegExp(' ' + cls + ' ').test(' ' + el.className + ' ');
    }
</script>

<script type="text/plain" class='optanon-category-C0004'>

