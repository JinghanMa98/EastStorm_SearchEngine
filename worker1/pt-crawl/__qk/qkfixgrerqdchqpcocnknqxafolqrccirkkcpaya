          transport: Sentry.makeMultiplexedTransport(
            Sentry.makeFetchTransport,
            (args) => {
              const event = args.getEvent();
              if (
                event &&
                event.extra &&
                EXTRA_KEY in event.extra &&
                Array.isArray(event.extra[EXTRA_KEY])
              ) {
                return event.extra[EXTRA_KEY];
              }
              return [];
            },
          ),
