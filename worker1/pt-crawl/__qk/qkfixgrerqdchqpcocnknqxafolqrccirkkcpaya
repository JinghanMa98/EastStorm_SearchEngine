                <script>
                // Hide paperback and hardback elements on initial load
                function hideNonDigitalElements() {
                  $('[data-isbn-type*="paperback"]').hide();
                  $('[data-isbn-type*="hardback"]').hide();
                }
                
                // Get format from query parameters
                function getFormatFromQueryParams() {
                  const urlParams = new URLSearchParams(window.location.search);
                  
                  if (urlParams.get('isDigital') === 'true') {
                    return 'digital';
                  } else if (urlParams.get('isHardback') === 'true') {
                    return 'hardback';
                  } else if (urlParams.get('isPaperback') === 'true') {
                    return 'paperback';
                  }
                  
                  return null;
                }
                
                // Show elements based on selected format
                function showFormatElements(selectedFormat) {
                  $('.isbn-spec').each(function() {
                    const $spec = $(this);
                    const isbnTypes = $spec.attr('data-isbn-type');
                    
                    if (isbnTypes && isbnTypes.includes(selectedFormat)) {
                      $spec.show();
                    } else {
                      $spec.hide();
                      }
                  });
                }
                
                $(document).ready(function() {
                  const formatFromQuery = getFormatFromQueryParams();
                  
                  if (formatFromQuery === 'digital') {
                    hideNonDigitalElements();
                  } else {
                    showFormatElements(formatFromQuery);
                  }
                  
                  // If no format is specified in query params, listen for bookFormatChanged event
                  if (!formatFromQuery) {
                      $(document).on('bookFormatChanged', function(event) {
                        const selectedFormat = event.originalEvent.detail.format;
                        showFormatElements(selectedFormat);
                      });
                  }
                  setTimeout(function() {
                      if (window.bookFormatSelector) {
                        const currentFormat = window.bookFormatSelector.getSelectedFormat();
                        const event = new CustomEvent('bookFormatChanged', {
                          detail: { format: currentFormat }
                        });
                        document.dispatchEvent(event);
                      }
                  }, 100);
                });
                </script>  <div class="space-divider desktop-tablet-only">
