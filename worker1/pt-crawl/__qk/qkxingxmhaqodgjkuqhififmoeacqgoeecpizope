	<script type="text/javascript">
	  businessDesc = function(){
	  	this.descFull = Element.get("bDesc");
	  	this.descShort = Element.get("bDescTeaser");
	  	this.swapLink = Element.get("bDescSwap");
	  }
	  
	  businessDesc.prototype.AttachEvents = function(){
	  	if(this.swapLink){
	  		Events.add({
	  			element: this.swapLink,
	  			type:    "click",
	  			handler: this.Swap,
	  			context: this
	  		});
	  	}
	  }
	  
	  businessDesc.prototype.Swap = function(){
	  	if(this.descFull.style.display == "none"){
	  		this.descShort.style.display = "none";
	  		this.descFull.style.display = "block";
	  		this.swapLink.innerHTML = "Hide Detailed Description";
	  	}
	  	else if(this.descShort.style.display == "none"){
	  		this.descFull.style.display = "none";
	  		this.descShort.style.display = "block";
	  		this.swapLink.innerHTML = "Detailed Description";
	  	}
	  }
	  
	  function InitBusinessDesc(){
	  	var bDesc = new businessDesc();
	  	bDesc.AttachEvents();
	  }
	  
	  Events.add({
	  	element: window,
	  	type:    "load",
	  	handler: InitBusinessDesc
	  });
	</script>


