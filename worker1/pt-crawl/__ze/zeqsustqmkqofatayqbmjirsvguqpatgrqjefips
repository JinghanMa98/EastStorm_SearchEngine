    <script>

function avvirGetConsent() {
	if (document.cookie.match(/cookie_notice_accepted=true/)) return true;

	var name = 'CookieInformationConsent';
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	var raw = "";
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) {
			raw = c.substring(nameEQ.length,c.length);
			break;
		}
	}
	if (raw) {
		var raw = decodeURIComponent(raw);
		try {
			var cooked = JSON.parse(raw);
			if (! cooked) return null;
			if (cooked.consents_approved.includes( 'cookie_cat_statistic')) return true;
		} catch (e) {
			return false;
		}
	}
	return false;
}


var k5aMeta= {
	"paid": 0,
	"paywall": "open",
	"subscriber":0,
	"login":0,
        "tag": [],
        "section": "",
        
        

}
if (avvirGetConsent()) {
    k5aMeta['consent'] = 1;
} else {
    k5aMeta['consent'] = 0;
}
</script>
